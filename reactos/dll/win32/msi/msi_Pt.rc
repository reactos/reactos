	"\t/y\n" \
	"Unregister MSI Service:\n" \
	"\t/z\n" \
	"Display this help:\n" \
	"\t/help\n" \
	"\t/?\n"
	11 "entre qual pasta contém %s"
	12 "instalar fonte para característica faltando"
	13 "drive de rede para  característica faltando"
