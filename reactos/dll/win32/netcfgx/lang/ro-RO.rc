    LISTBOX IDC_DNSSUFFIXLIST, 5, 155, 180, 50, LBS_NOTIFY
    PUSHBUTTON "S&us", IDC_DNSSUFFIXUP, 190, 160, 50, 14, WS_TABSTOP
    PUSHBUTTON "J&os", IDC_DNSSUFFIXDOWN, 190, 180, 50, 14, WS_TABSTOP
