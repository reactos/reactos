  LTEXT "Ниже", -1, 40, 185, 30, 10
  LTEXT "Выше", -1, 200, 185, 30, 10
  CONTROL "",IDC_SLIDER_CURSOR_BLINK, "msctls_trackbar32", TBS_AUTOTICKS | WS_TABSTOP, 65, 185, 130, 17
