    BEGIN
        MENUITEM "Grup nou…", IDM_GROUP_NEW
    END
    POPUP ""
    BEGIN
        MENUITEM "Adaugă membru", IDM_GROUP_ADD_MEMBER, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "Șterge", IDM_GROUP_DELETE
        MENUITEM "Redenumește", IDM_GROUP_RENAME
        MENUITEM SEPARATOR
        MENUITEM "Proprietăți", IDM_GROUP_PROPERTIES
    END
END

IDM_POPUP_USER MENU
