IDD_PROPPAGEINSTALL DIALOGEX 0, 0, PROPSHEETWIDTH, PROPSHEETHEIGHT
STYLE DS_SHELLFONT | WS_CHILD | WS_DISABLED | WS_CAPTION
CAPTION "Зміна й видалення програм"
FONT 8, "MS Shell Dlg", 0, 0, 0x0
BEGIN
	ICON IDI_CPLSYSTEM,-1,PROPSHEETPADDING,6,16,16
	LTEXT "Нижченаведені програми можна видалити або переустановити автоматично. Щоб видалити програму або змінити склад установлених компонентів, виберіть цю програму зі списку й натисніть кнопку ""Змінити/Видалити"".",-1,36,7,PROPSHEETWIDTH-48,22
	LTEXT "Пошук:",-1,PROPSHEETPADDING,33,30,8
	EDITTEXT IDC_FIND_EDIT, PROPSHEETPADDING+30, 30, 180, 14, WS_VISIBLE | WS_TABSTOP
	LTEXT "Вигляд:",-1,250,33,30,8
	COMBOBOX IDC_VIEW_COMBO, 280, 30, 94, 80, CBS_DROPDOWNLIST | WS_CHILD | WS_VISIBLE | WS_TABSTOP | WS_VSCROLL
	CONTROL "", IDC_SOFTWARELIST, "SysListView32", LVS_REPORT | LVS_SORTASCENDING | LVS_AUTOARRANGE | LVS_SINGLESEL, PROPSHEETPADDING, 50, PROPSHEETWIDTH-12, 155, WS_EX_CLIENTEDGE
	PUSHBUTTON "Змінити/В&идалити", IDC_ADDREMOVE, PROPSHEETPADDING, 210, 80, 14, WS_DISABLED
	PUSHBUTTON "&Відомості", IDC_INFO_BUTTON, PROPSHEETPADDING+85, 210, 80, 14, WS_DISABLED
END

