LANGUAGE LANG_UKRAINIAN, SUBLANG_DEFAULT

IDD_MONITOR DIALOGEX 0, 0, 252, 226
STYLE DS_SHELLFONT | WS_CHILD | WS_DISABLED | WS_CAPTION
