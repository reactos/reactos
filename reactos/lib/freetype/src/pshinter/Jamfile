# FreeType 2 src/pshinter Jamfile (c) 2001 David Turner
#

SubDir  FT2_TOP $(FT2_SRC_DIR) pshinter ;

{
  local  _sources ;

  if $(FT2_MULTI)
  {
    _sources = pshrec pshglob pshalgo pshmod ;
  }
  else
  {
    _sources = pshinter ;
  }

  Library  $(FT2_LIB) : $(_sources).c ;
}

# end of src/pshinter Jamfile
