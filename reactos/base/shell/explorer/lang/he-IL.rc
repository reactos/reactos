        MENUITEM "הצג חולונות זה לצד זה", ID_SHELL_CMD_TILE_WND_V
        MENUITEM "הצג את שולחן העבודה", ID_SHELL_CMD_SHOW_DESKTOP
        MENUITEM "בטל", ID_SHELL_CMD_UNDO_ACTION
