LANGUAGE LANG_ROMANIAN, SUBLANG_NEUTRAL

IDR_MAINMENU MENU
BEGIN
	POPUP "&Fișier"
	BEGIN
		MENUITEM "&Deschidere…", IDM_OPEN_FILE
		MENUITEM "Î&nchide",     IDM_CLOSE_FILE
		MENUITEM SEPARATOR
		MENUITEM "I&eșire",      IDM_EXIT
	END
	POPUP "&?"
	BEGIN
		MENUITEM "&Despre…",     IDM_ABOUT
	END
END

