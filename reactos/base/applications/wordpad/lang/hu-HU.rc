    CHECKBOX "&Format Bar", IDC_PAGEFMT_FB, 165, 38, 80, 15
    CHECKBOX "&Ruler", IDC_PAGEFMT_RU, 165, 56, 80, 15
    CHECKBOX "&Status Bar", IDC_PAGEFMT_SB, 165, 74, 80, 15
