STID_INDEX, 		"Inde&x"
STID_CONTENTS,		"&Inhoudsopgave"
STID_BACK, 		"&Terug"
STID_ALL_FILES, 	"Alle bestanden (*.*)"
STID_HELP_FILES_HLP, 	"Helpbestanden (*.hlp)"
