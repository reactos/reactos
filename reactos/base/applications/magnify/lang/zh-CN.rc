    CONTROL "跟随键盘焦点", IDC_FOLLOWKEYBOARDCHECK, "Button",
            BS_AUTOCHECKBOX | WS_TABSTOP, 18, 54, 114, 10
    CONTROL "跟随文本编辑", IDC_FOLLOWTEXTEDITINGCHECK, "Button",
