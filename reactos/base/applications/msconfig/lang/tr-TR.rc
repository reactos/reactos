    CONTROL "",IDC_SCR_CHANNEL, "msctls_updown32", 0x50000000, 140, 130, 11, 11
    PUSHBUTTON "Tamam", IDC_OK, 20, 160, 50, 12
    PUSHBUTTON "Vazgeç", IDC_CANCEL, 100, 160, 50, 12
END

