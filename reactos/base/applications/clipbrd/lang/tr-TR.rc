END

MAIN_MENU MENU
BEGIN
    POPUP "&Kütük"
    BEGIN
        MENUITEM "&Aç...", CMD_OPEN
        MENUITEM "A&yrı Sakla...", CMD_SAVE_AS
        MENUITEM SEPARATOR
        MENUITEM "&Çıkış", CMD_EXIT
    END
    POPUP "&Düzen"
    BEGIN
        MENUITEM "&Sil\tDel", CMD_DELETE
    END
    POPUP "&Görüntü"
    BEGIN
        MENUITEM "&Kendiliğinden", CMD_AUTOMATIC
    END
    POPUP "&Yardım"
    BEGIN
        MENUITEM "&Yardım Konuları", CMD_HELP
        MENUITEM SEPARATOR
        MENUITEM "&Üzerine", CMD_ABOUT
    END
END

