LANGUAGE LANG_CHINESE, SUBLANG_CHINESE_SIMPLIFIED

IDM_CONSOLE_SMALL MENU
BEGIN
    POPUP "档案(&F)"
    BEGIN
        MENUITEM "新建\tCtrl+N", IDM_FILE_NEW
        MENUITEM SEPARATOR
        MENUITEM "结束(&X)\tAlt+F4", IDM_FILE_EXIT
    END
    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "关于 ReactOS 控制台(&A)...", IDM_HELP_ABOUT
    END
END

IDM_CONSOLE_LARGE MENU
BEGIN
    POPUP "档案(&F)"
    BEGIN
        MENUITEM "新建\tCtrl+N", IDM_FILE_NEW
        MENUITEM SEPARATOR
        MENUITEM "结束(&X)\tAlt+F4", IDM_FILE_EXIT
    END
    POPUP "窗口(&W)"
    BEGIN
        MENUITEM "列表", IDM_WINDOWS_LIST
    END
    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "关于 ReactOS 控制台(&A)...", IDM_HELP_ABOUT
    END
END

STRINGTABLE
BEGIN
    IDS_APPTITLE "ReactOS 控制台"
    IDS_CONSOLETITLE "控制台%1!u!"
END
