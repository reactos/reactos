        MENUITEM "New\tCtrl+N", IDM_FILE_NEW
        MENUITEM SEPARATOR
        MENUITEM "종료(&E)\tAlt+F4", IDM_FILE_EXIT
    END
    POPUP "도움말(&H)"
    BEGIN
        MENUITEM "ReactOS 관리 콘솔에 대하여...(&A)", IDM_HELP_ABOUT
    END
END

