    BEGIN
        MENUITEM "New\tStrg+N", IDM_FILE_NEW
        MENUITEM SEPARATOR
        MENUITEM "&Çıkış\tAlt+F4", IDM_FILE_EXIT
    END

    POPUP "&Windows"
    BEGIN
        MENUITEM "List", IDM_WINDOWS_LIST
    END

    POPUP "&Yardım"
    BEGIN
        MENUITEM "&Hakkında...", IDM_HELP_ABOUT
    END
END

