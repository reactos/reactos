    IDS_STRDUR              "Längd: %.2f s"
    IDS_STRBUF              "Buffert: %.2f kb"
    IDS_STRFMT              "%.1f kHz %u bits"
    IDS_STRMONO             "mono"
    IDS_STRSTEREO           "stereo"
    IDS_STRCHAN             "%s"
