MAJORCOMP=msxml
MINORCOMP=schema

ROOT=$(MAKEDIR:\xml\schema=)
!include $(ROOT)\common.inc

TARGETNAME=schema
TARGETPATH=obj
TARGETTYPE=LIBRARY

SOURCES= \
    schemanames.cxx \
    schemanodefactory.cxx \
    schemabuilder.cxx \

INCLUDES= \
	$(ROOT)\include; \
	$(ROOT); \
    $(ROOT)\xml\parser; \
    $(ROOT)\xml\om; \
    $(ROOT)\core\base; \
    $(ROOT)\core\util; \
    $(ROOT)\xml\dtd; \
    $(WINDOWS_INC_PATH)\

!ifdef WIN16
PRECOMPILED_OPTION=/fhr /fh=$(ROOT)\include\$(O)\core.pch
!else
PRECOMPILED_OPTION=/Yucore.hxx /Fp$(ROOT)\include\$(O)\core.pch
!endif
PRECOMPILED_CXX=1
CONDITIONAL_INCLUDES=\
    $(CONDITIONAL_INCLUDES)\
	Document.hxx\

