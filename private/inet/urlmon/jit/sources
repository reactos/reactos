# Files that have the same name, but are different for each platform

COMMON_MISCFILES = \
    iejit.htx    \
    jitalert.gif



# Complete file lists

!if $(386)
MISCFILES = \
    $(MISCFILES) \
    $(COMMON_MISCFILES) \
    $(X86_MISCFILES)

!elseif $(MIPS)
MISCFILES = \
    $(MISCFILES) \
    $(COMMON_MISCFILES) \
    $(MIPS_MISCFILES)

!elseif $(PPC)
MISCFILES = \
    $(MISCFILES) \
    $(COMMON_MISCFILES) \
    $(PPC_MISCFILES)

!elseif $(ALPHA)
MISCFILES = \
    $(MISCFILES) \
    $(COMMON_MISCFILES) \
    $(ALPHA_MISCFILES)
!endif
