    DECLARE_HANDLE(HBODY);
    typedef HBODY *LPHBODY;
    DECLARE_HANDLE(HHEADERROW);
    typedef HHEADERROW *LPHHEADERROW;

    typedef DWORD TYPEDID;

    typedef const PROPVARIANT *LPCPROPVARIANT;
