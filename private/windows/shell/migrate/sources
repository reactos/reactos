    $(BASEDIR)\public\sdk\lib\*\setargv.obj \
    $(BASEDIR)\public\sdk\lib\*\userenv.lib \
    $(BASEDIR)\public\sdk\lib\*\user32.lib \
    $(BASEDIR)\public\sdk\lib\*\shlwapi.lib \
