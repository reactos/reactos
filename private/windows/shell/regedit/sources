NTLEANANDMEAN=



MAJORCOMP=shell
MINORCOMP=regedit

TARGETNAME=regedit
TARGETPATH=obj
TARGETTYPE=PROGRAM
TARGETLIBS=

NOT_LEAN_AND_MEAN=1

USE_LIBCMT=1

C_DEFINES=-DNT -DWINNT -DUNICODE -D_UNICODE

!IF "$(SHELLDBG)"=="1"
C_DEFINES=$(C_DEFINES) -DDEBUG
!ENDIF

INCLUDES=.;..\inc;..\..\inc;$(BASEDIR)\private\shell\inc

MSC_WARNING_LEVEL=/W3 /WX

SOURCES= \
         regbined.c \
         regcdhk.c  \
         regdebug.c \
         regdwded.c \
         regedit.c  \
         regfile.c  \
         regfind.c  \
         regkey.c   \
         regmain.c  \
         regmisc.c  \
         regnet.c   \
         regporte.c \
         regprint.c \
         regstred.c \
         regvalue.c

UMTYPE=windows
EXPECTED_WINVER=4.0
!IF $(386)
UMENTRYABS=ModuleEntry@0
!ELSE
UMENTRYABS=ModuleEntry
!ENDIF
UMRES=obj\*\regedit.res
UMLIBS= \
        $(BASEDIR)\public\sdk\lib\*\kernel32.lib \
        $(BASEDIR)\public\sdk\lib\*\gdi32.lib   \
        $(BASEDIR)\public\sdk\lib\*\user32.lib  \
        $(BASEDIR)\public\sdk\lib\*\advapi32.lib \
        $(BASEDIR)\public\sdk\lib\*\comctl32.lib \
        $(BASEDIR)\public\sdk\lib\*\comctlp.lib  \
        $(BASEDIR)\public\sdk\lib\*\comdlg32.lib \
        $(BASEDIR)\public\sdk\lib\*\htmlhelp.lib \
        $(BASEDIR)\public\sdk\lib\*\shlwapi.lib  \
        $(BASEDIR)\public\sdk\lib\*\uuid.lib  \
        $(BASEDIR)\public\sdk\lib\*\shell32.lib  \
        $(BASEDIR)\public\sdk\lib\*\shell32p.lib

PRECOMPILED_INCLUDE=pch.h
PRECOMPILED_PCH=pch.pch
PRECOMPILED_OBJ=pch.obj
