MAJORCOMP=shell
MINORCOMP=accesory

TARGETNAME=notepad
TARGETPATH=obj
TARGETTYPE=LIBRARY
TARGETLIBS=

INCLUDES=..\..\userpri;..\..\..\inc

USE_CRTDLL=1

SOURCES=notepad.rc   \
        notepad.c    \
        npinit.c     \
        npfile.c     \
        npmisc.c     \
        npprint.c    \
        npdate.c     \
        nputf.c


C_DEFINES=-DWIN32 -DNT -DUNICODE -D_UNICODE

UMTYPE=windows
UMENTRY=winmain
UMAPPL=notepad
UMRES=obj\*\notepad.res
UMLIBS=$(BASEDIR)\public\sdk\lib\*\comdlg32.lib  \
       $(BASEDIR)\public\sdk\lib\*\shell32.lib   \
       $(BASEDIR)\public\sdk\lib\*\htmlhelp.lib  \
       $(BASEDIR)\public\sdk\lib\*\winspool.lib  \
       obj\*\notepad.lib

PRECOMPILED_INCLUDE=precomp.h
PRECOMPILED_PCH=precomp.pch
PRECOMPILED_OBJ=precomp.obj
