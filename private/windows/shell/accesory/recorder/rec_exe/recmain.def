NAME
DESCRIPTION 'Windows Macro Recorder'

STUB	'WINSTUB.EXE'

EXETYPE WINDOWS
CODE    MOVEABLE
DATA    MOVEABLE MULTIPLE

HEAPSIZE    2048
STACKSIZE   8192

;STACKSIZE   6000   old value

SEGMENTS
    _TEXT		MOVEABLE PRELOAD	DISCARDABLE
    RECORDER_TEXT 	MOVEABLE PRELOAD	DISCARDABLE
    RECORD_TEXT 	MOVEABLE LOADONCALL 	DISCARDABLE
    DLG_TEXT 		MOVEABLE LOADONCALL 	DISCARDABLE
    UTIL_TEXT 		MOVEABLE LOADONCALL 	DISCARDABLE
    RINIT_TEXT		MOVEABLE LOADONCALL	DISCARDABLE
    FILEIO_TEXT 	MOVEABLE LOADONCALL	DISCARDABLE
    UTILC_TEXT		MOVEABLE PRELOAD	DISCARDABLE

EXPORTS
    WndProc	   @1
    dlgOpen	   @2
    dlgSave	   @3
    dlgRecord	   @4
    dlgAbout	   @5
    dlgEdit	   @6
    EnumProc	   @7
    dlgMerge	   @8
    dlgPref	   @9
    dlgPBError	   @10
    dlgInterrupt   @11
    dlgCmds	   @12
    KeyWndProc	   @13
