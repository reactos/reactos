/*
 * Dialog for LINK Properties...
 */
DTLINKS DIALOG LOADONCALL MOVEABLE DISCARDABLE 12, 34, 291, 97
CAPTION "Link"
FONT 8 "MS Shell Dlg"
STYLE WS_BORDER | WS_CAPTION | WS_DLGFRAME | WS_SYSMENU | WS_VISIBLE | DS_MODALFRAME | WS_POPUP
BEGIN
    /* List box full of links and objects, for Cardfile only one link is active */
    CONTROL "Link:", -1, "button", BS_GROUPBOX | WS_CHILD, 4, 1, 238, 26
    CONTROL "", IDD_CLASSID, "static", SS_LEFT | WS_CHILD, 5, 12, 80, 8
    CONTROL "", IDD_DOCINFO, "static", SS_LEFT | WS_CHILD, 85, 12,  80, 8
    CONTROL "", IDD_ITEMINFO, "static", SS_LEFT | WS_CHILD, 165, 12, 69, 8

    /* Automatic and manual link options */
    CONTROL "Update:", -1, "static", SS_LEFT | WS_CHILD, 8, 46, 30, 9
    CONTROL "Au&tomatic", IDD_AUTO, "button", BS_RADIOBUTTON | WS_GROUP | WS_TABSTOP | WS_CHILD, 39, 44, 46, 14
    CONTROL "&Manual", IDD_MANUAL, "button", BS_RADIOBUTTON | WS_TABSTOP | WS_CHILD, 89, 44, 42, 14

    /* Push buttons at the bottom of the dialog */
    CONTROL "&Update Now", IDD_UPDATE, "button", BS_PUSHBUTTON | WS_TABSTOP | WS_CHILD, 26, 63, 58, 14
    CONTROL "Ca&ncel Link", IDD_FREEZE, "button", BS_PUSHBUTTON | WS_TABSTOP | WS_CHILD, 91, 63, 58, 14
    CONTROL "C&hange Link...", IDD_CHANGE, "button", BS_PUSHBUTTON | WS_TABSTOP | WS_CHILD, 156, 63, 58, 14

    /* Push buttons on the side of the dialog */
    CONTROL "OK", IDOK, "button", BS_PUSHBUTTON | WS_TABSTOP | WS_CHILD, 249, 6, 37, 14
    CONTROL "Cancel", IDCANCEL, "button", BS_PUSHBUTTON | WS_TABSTOP | WS_CHILD, 249, 25, 37, 14
    CONTROL "&Activate", IDD_PLAY, "button", BS_PUSHBUTTON | WS_TABSTOP | WS_CHILD, 249, 44, 37, 14
    CONTROL "&Edit", IDD_EDIT, "button", BS_DEFPUSHBUTTON | WS_GROUP | WS_TABSTOP | WS_CHILD, 249, 63, 37, 14
END

