MAJORCOMP=shell
MINORCOMP=games

TARGETNAME=reversi
TARGETPATH=obj
TARGETTYPE=LIBRARY
TARGETLIBS=

INCLUDES=.

C_DEFINES=-DWIN32

USE_CRTDLL=1

SOURCES=reversi.c rev.c res.rc

UMTYPE=windows
UMENTRY=winmain
UMAPPL=reversi
UMLIBS=\nt\public\sdk\lib\*\user32.lib                 \
       \nt\public\sdk\lib\*\ntdll.lib                  \
       \nt\public\sdk\lib\*\gdi32.lib                  \
       \nt\public\sdk\lib\*\pwin32.lib               \
       obj\*\rev.obj                                 \
       obj\*\res.res
