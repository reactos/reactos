TARGETNAME=cryptscn
TARGETPATH=obj
TARGETTYPE=PROGRAM

UMTYPE=console

INCLUDES=   $(BASEDIR)\private\windows\base\ntcrypto\inc;

SOURCES= cryptscn.c

TARGETLIBS= \
       $(BASEDIR)\private\lib\*\rsa32.lib \
       $(BASEDIR)\public\sdk\lib\*\user32.lib \
       $(BASEDIR)\public\sdk\lib\*\advapi32.lib

C_DEFINES=$(C_DEFINES) -DRPC_NO_WINDOWS_H -DUNICODE

