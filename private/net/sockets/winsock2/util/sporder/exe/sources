TARGETNAME=sporder
TARGETPATH=obj
TARGETTYPE=PROGRAM

TARGETLIBS=$(BASEDIR)\public\sdk\lib\*\kernel32.lib \
           $(BASEDIR)\public\sdk\lib\*\comctl32.lib \
           $(BASEDIR)\public\sdk\lib\*\sporder.lib  \
           $(BASEDIR)\public\sdk\lib\*\ws2_32.lib

INCLUDES=.

