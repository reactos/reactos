UMTYPE=console
UMLIBS=\
    $(TARGETPATH)\*\$(TARGETNAME).lib   \
    $(LIBRARY_PATH)\ntdll.lib           \
    $(LIBRARY_PATH)\gdi32.lib           \
    $(LIBRARY_PATH)\user32.lib          \
    $(LIBRARY_PATH)\ws2_32.lib
