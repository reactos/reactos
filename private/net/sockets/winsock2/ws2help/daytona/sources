TARGETPATH=$(BASEDIR)\public\sdk\lib
DLLENTRY=DllMain
SYNCHRONIZE_BLOCK=1

!INCLUDE ..\sources.inc
TARGETLIBS=$(TARGETLIBS)                        \
    $(BASEDIR)\public\sdk\lib\*\ntdll.lib       \
    $(BASEDIR)\public\sdk\lib\*\user32.lib      \

DELAYLOAD=user32.dll
USE_NTDLL=1

SOURCES=\
    ..\ws2help.rc                               \
    apc.c                                       \
    handle.c                                    \
    notify.c                                    \
    newctx.c                                    \


NTPROFILEINPUT=yes
!UNDEF NTTARGETFILE0

