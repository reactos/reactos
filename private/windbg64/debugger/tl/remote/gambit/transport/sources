!INCLUDE ..\..\..\..\sources.inc

MAJORCOMP=dbg
MINORCOMP=tlgambit

TARGETNAME=tlgambit
TARGETPATH=obj
TARGETTYPE=DYNLINK
DLLDEF=tlgambit.def
COFFBASE=tlgambit

SOURCES=tlgambit.c   \
	xport.c	\
        	tlgambit.rc

UMTYPE=windows

C_DEFINES=$(C_DEFINES) -DWIN32 -DTARGET32 -DHOST32 -DADDR_MIXED -DOSDEBUG4

INCLUDES=..\xport;\
        $(LANGAPI_ROOT)\debugger;\
        $(DEBUGGER_ROOT)\include;\
        $(DEBUGGER_ROOT)\od\include;\
        $(DEBUG_INCLUDES)


TARGETLIBS=$(MORE_LIBS) \
           $(SDK_LIB_PATH)\kernel32.lib \
           $(SDK_LIB_PATH)\user32.lib
