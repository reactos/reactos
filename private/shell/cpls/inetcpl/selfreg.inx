;
;  Control registration and DLL installation
;
;  Note the AddReg and DelReg keys have a maximum length of 255 characters.
;  Only one AddReg and DelReg key per header section.
;
;  Because of the length limitations, we use shorthand names for the
;  sections.
;
;  Section Name Notations:
;       A_*     - add reg section
;       D_*     - del reg section
;       AD_*    - add/del reg section
;
;
;  RegItems with %11% or %25% are substituted on NT with
;  %SystemRoot%\whatever. But this needs to be REG_EXPAND_SZ!
;  The flag 131072 (0x20000) will make the item be of type
;  REG_EXPAND_SZ.
;
;  Any entries that go into the registry that contain
;  %_SYS_MOD_PATH%, %11%, %25% MUST have this flag set. (w95 ignores it).

;NOTE: 0x8000002 is HKEY_LOCAL_MACHINE
;      0x8000001 is HKEY_CURRENT_USER


[Version]
Signature="$CHICAGO$"

[RegDll]
AddReg=ao2_reg_hklm,so_reg_hklm,so_reg_hklm_adm
DelReg=ao2_old
RequiredEngine=SETUPAPI,%ERR_NOSETUPAPI%

[RegDll.IntegratedShell]
AddReg=ao2_reg_hklm_intshell
RequiredEngine=SETUPAPI,%ERR_NOSETUPAPI%

[RegDll.NT5]
AddReg=add_NT5
DelReg=del_NT5
RequiredEngine=SETUPAPI,%ERR_NOSETUPAPI%

[UnregDll]
DelReg=net_del
RequiredEngine=SETUPAPI,%ERR_NOSETUPAPI%

[net_del]
HKLM,"%SMIEAO%"
HKLM,%SO%
HKLM,%SOIEAK%


[ao2_old]
;; get rid of garbage
; HKLM,"Software\Microsoft\Internet Explorer\Advanced Options"
; HKCU,"Software\Microsoft\Internet Explorer\Advanced Options"
HKCU,"%SMIEAO%"
HKLM,"%SMIEAO%\TB"

; In Beta 1 & 2, but not needed for RTM
HKLM,"%SMIEAO%\CRYPTO\%R%"
HKLM,"%SMIEAO%\CRYPTO\FLOPPY"

; Cookies were moved to zones, need to delete the reg key so it doesn't show up in the
; advanced tab
HKLM,%SMIEAO%\CRYPTO\COOKIES
HKLM,%SO%\MISC\%CO%
HKLM,%SOIEAK%\MISC\%CO%

; AutoSuggest moved to Content tab, delete Advanced checkboxes
HKLM,%SMIEAO%\BROWSE\FORMSUGGEST

; AutoComplete for Integrated Shell selectively added if needed.
HKLM,%SMIEAO%\BROWSE\AUTOCOMPLETE

; Full Screen Modes no longer an option in Advanced checkboxes
HKLM,%SMIEAO%\BROWSE\FULLSCREEN
HKLM,%SMIEAO%\BROWSE\CHANNELFULLSCREEN

; Autosearch/autoscan options changed so nuke the old ones
HKLM,%SMIEAO%\%S%

; Don't show the tour option post IE4.
HKLM,%SMIEAO%\BROWSE\SHOWIE4

; Don't show Launch browser window in new process option post IE5
HKLM,%SMIEAO%\%B%\%BNP%

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;
;; NT5 specific setup.  This section is called last during install
;; (after all other sections.
;;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[add_NT5]
; Update description for NT5
HKLM,%SMIEAO%\%B%\AUTOAPPEND,"PlugUIText",,"@inetcplc.dll,-4852"

[del_NT5]
; On NT5 we don't use the following keys
HKLM,%SMIEAO%\%B%\AUTOCOMPLETE

