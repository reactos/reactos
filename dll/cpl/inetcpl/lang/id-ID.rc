                    IDC_DELETE_PASSWORDS, 10, 168, 230, 40, BS_TOP | BS_MULTILINE
    DEFPUSHBUTTON  "Batal", IDCANCEL, 185, 230, 60, 15, WS_GROUP
    PUSHBUTTON     "&Hapus", IDOK, 120, 230, 60, 15, WS_GROUP

END

