STRINGTABLE
BEGIN
    IDS_CPL_NAME        "網際網路選項"
    IDS_CPL_INFO        "配置 Wine 網際網路瀏覽器和相關的設定"
    IDS_SEC_SETTINGS    "區域的安全設定︰"
    IDS_SEC_LEVEL0      "自訂"
    IDS_SEC_LEVEL1      "非常低"
    IDS_SEC_LEVEL2      "低"
    IDS_SEC_LEVEL3      "中等"
    IDS_SEC_LEVEL4      "增大"
    IDS_SEC_LEVEL5      "高"
