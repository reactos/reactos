        MENUITEM "&Sil", IDM_USER_DELETE
        MENUITEM "&Yeniden Adlandır", IDM_USER_RENAME
        MENUITEM SEPARATOR
        MENUITEM "Ö&zellikler", IDM_USER_PROPERTIES
    END
END

