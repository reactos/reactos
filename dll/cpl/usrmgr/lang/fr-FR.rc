/* Menus */

IDM_POPUP_GROUP MENU
BEGIN
    POPUP ""
    BEGIN
        MENUITEM "Nouveau groupe...", IDM_GROUP_NEW
    END
    POPUP ""
    BEGIN
        MENUITEM "Ajouter membre", IDM_GROUP_ADD_MEMBER, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "Supprimer", IDM_GROUP_DELETE
        MENUITEM "Renommer", IDM_GROUP_RENAME
        MENUITEM SEPARATOR
        MENUITEM "Propriétés", IDM_GROUP_PROPERTIES
    END
END

IDM_POPUP_USER MENU
BEGIN
    POPUP ""
    BEGIN
        MENUITEM "Nouvel utilisateur...", IDM_USER_NEW
    END
    POPUP ""
    BEGIN
        MENUITEM "Changer le mot de passe", IDM_USER_CHANGE_PASSWORD
        MENUITEM SEPARATOR
        MENUITEM "Supprimer", IDM_USER_DELETE
        MENUITEM "Renommer", IDM_USER_RENAME
        MENUITEM SEPARATOR
        MENUITEM "Propriétés", IDM_USER_PROPERTIES
    END
END

