        MENUITEM "Löschen", IDM_USER_DELETE
        MENUITEM "Umbenennen", IDM_USER_RENAME
        MENUITEM SEPARATOR
        MENUITEM "Eigenschaften", IDM_USER_PROPERTIES
    END
END

