    AUTOCHECKBOX "使用者必須在首次登入後更改密碼", IDC_USER_GENERAL_FORCE_CHANGE, 7, 82, 210, 10
    AUTOCHECKBOX "使用者不能更改密碼", IDC_USER_GENERAL_CANNOT_CHANGE, 7, 95, 210, 10
    AUTOCHECKBOX "密碼永不過期", IDC_USER_GENERAL_NEVER_EXPIRES, 7, 108, 210, 10
