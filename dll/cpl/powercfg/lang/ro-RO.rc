    LTEXT "Când computerul este alimentat:", IDC_SAT, 12, 110, 60, 15
    LTEXT "De la reţeaua electrică", IDC_SAC, 100, 110, 50, 15
    ICON IDI_AC, IDC_IAC, 141, 122, 0, 0
    LTEXT "De la baterii", IDC_SDC, 173, 110, 50, 15
