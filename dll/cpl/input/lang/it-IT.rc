    AUTORADIOBUTTON "A&LT sinistro", IDC_LEFT_ALT_LAYOUT, 30, 95, 44, 12
    DEFPUSHBUTTON "OK", IDOK, 172, 9, 50, 14
    PUSHBUTTON "Annulla", IDCANCEL, 172, 27, 50, 14
END

