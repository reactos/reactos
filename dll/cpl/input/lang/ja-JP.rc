    AUTORADIOBUTTON "左A&LT", IDC_LEFT_ALT_LAYOUT, 30, 95, 44, 12
    DEFPUSHBUTTON "OK", IDOK, 172, 9, 50, 14
    PUSHBUTTON "キャンセル", IDCANCEL, 172, 27, 50, 14
END

