    AUTORADIOBUTTON "CAPS &LOCKキーを押す", IDC_PRESS_CL_KEY_RB, 14, 17, 120, 11, WS_GROUP
    AUTORADIOBUTTON "SHIFTキーを押す(&F)", IDC_PRESS_SHIFT_KEY_RB, 144, 17, 120, 11, NOT WS_TABSTOP
