    LTEXT "Exemples", -1, 14, 73, 70, 10
    LTEXT "Nombre:", -1, 16, 86, 48, 10
    LTEXT "Monnaie:", -1, 16, 101, 48, 10
    LTEXT "Heure:", -1, 16, 116, 48, 10
    LTEXT "Date courte:", -1, 16, 131, 48, 10
    LTEXT "Date longue:", -1, 16, 146, 48, 10
