    LTEXT "Duże", -1, 40, 30, 20, 10
    LTEXT "Małe", -1, 200, 30, 20, 10
    CONTROL "", IDC_SLIDER_REPEAT_DELAY, "msctls_trackbar32", TBS_AUTOTICKS | WS_TABSTOP, 65, 30, 130, 17
