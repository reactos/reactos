    PUSHBUTTON      "&Annuler",IDCANCEL,240,114,60,14
    LTEXT           "Available modes",-1,6,6,52,8
    LTEXT           "Selected modes",-1,168,6,51,8
END

STRINGTABLE
BEGIN
    IDS_COMPAT_TITLE "Compatibilité"
