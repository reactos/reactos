    PUSHBUTTON "Cancel", IDCANCEL, 167, 115, 50, 15
    GROUPBOX "List of valid modes", -1, 6, 7, 212, 98
    LISTBOX IDC_ALLVALIDMODES, 10, 20, 204, 87, LBS_NOTIFY | WS_VSCROLL
END

