    LTEXT "Тип адреси:", -1, 22, 20, 80, 8
    LTEXT "IP-адреса:", -1, 22, 34, 80, 8
    LTEXT "Маска підмережі:", -1, 22, 48, 80, 8
    LTEXT "Основний шлюз:", -1, 22, 62, 80, 8
