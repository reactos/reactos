    ICON "", IDC_NETSTAT, 110, 85, 32, 32
    LTEXT "Отримано", -1, 149, 90, 37, 8
    LTEXT "Байтів:", -1, 17, 115, 32, 8
    RTEXT "000.000.000", IDC_SEND, 63, 115, 44, 8
    RTEXT "000.000.000", IDC_RECEIVED, 139, 115, 44, 8
