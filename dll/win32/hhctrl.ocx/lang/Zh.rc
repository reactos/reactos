END

STRINGTABLE
BEGIN
    IDTB_EXPAND      "显示"
    IDTB_CONTRACT    "隐藏"
    IDTB_STOP        "停止"
    IDTB_REFRESH     "刷新"
    IDTB_BACK        "向后"
    IDTB_HOME        "首页"
    IDTB_SYNC        "同步"
    IDTB_PRINT       "打印"
    IDTB_OPTIONS     "选项"
