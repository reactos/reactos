LANGUAGE LANG_CHINESE, SUBLANG_CHINESE_SIMPLIFIED

STRINGTABLE
BEGIN
#ifdef __REACTOS__
    IDS_DEFTITLE     "HTML 帮助"
#endif
    IDS_CONTENTS     "内容(&C)"
    IDS_INDEX        "索引(&N)"
    IDS_SEARCH       "搜索(&S)"
    IDS_FAVORITES    "收藏(&I)"

    IDS_HIDETABS     "隐藏选项卡(&T)"
