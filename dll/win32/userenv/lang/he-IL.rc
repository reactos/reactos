    IDS_SENDTO "שלח אל"
    IDS_TEMPLATES "תבניות"
    IDS_STARTMENU "תפריט ההתחלה"
    IDS_PROGRAMS "תפריט ההתחלה\\תכניות"
    IDS_STARTUP "תפריט ההתחלה\\תכניות\\הפעלה"
    IDS_LOCALSETTINGS "Local Settings"
