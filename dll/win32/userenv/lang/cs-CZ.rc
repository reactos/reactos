    IDS_RECENT "Poslední dokumenty"
    IDS_SENDTO "SendTo"
    IDS_TEMPLATES "Šablony"
    IDS_STARTMENU "Nabídka Start"
    IDS_PROGRAMS "Nabídka Start\\Programy"
    IDS_STARTUP "Nabídka Start\\Programy\\Po spuštění"
    IDS_LOCALSETTINGS "Local Settings"
