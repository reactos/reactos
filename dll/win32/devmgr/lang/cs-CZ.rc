/*
 This code relates to the device manager GUI
 */

IDM_MAINMENU MENU
BEGIN
    POPUP "&Soubor"
    BEGIN
        MENUITEM "U&končit",                       IDM_EXIT
    END
    POPUP "Akce"
    BEGIN
        MENUITEM "Vyhledat změny hardwaru",        IDM_SCAN_HARDWARE
        MENUITEM "Přidat hardware",                IDM_ADD_HARDWARE
    END
    POPUP "Zobrazit"
    BEGIN
        MENUITEM "Zařízení podle typu",             IDM_DEVBYTYPE
        MENUITEM "Zařízení podle připojení",       IDM_DEVBYCONN
        MENUITEM "Prostředky podle typu",           IDM_RESBYTYPE, GRAYED
        MENUITEM "Prostředky podle připojení",     IDM_RESBYCONN, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "SZobrazit skrytá zařízení",         IDM_SHOWHIDDEN
    END
    POPUP "Nápověda"
    BEGIN
        MENUITEM "O programu",                       IDM_ABOUT
    END
END


