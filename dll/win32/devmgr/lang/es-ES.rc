/*
 This code relates to the device manager GUI
 */

IDM_MAINMENU MENU
BEGIN
    POPUP "&Archivo"
    BEGIN
        MENUITEM "&Salir",                       IDM_EXIT
    END
    POPUP "Acción"
    BEGIN
        MENUITEM "Buscar cambios de hardware",   IDM_SCAN_HARDWARE
        MENUITEM "Añadir dispositivos",          IDM_ADD_HARDWARE
    END
    POPUP "Ver"
    BEGIN
        MENUITEM "Dispositivos por tipo",        IDM_DEVBYTYPE
        MENUITEM "Dispositivos por conexión",    IDM_DEVBYCONN
        MENUITEM "Recursos por tipo",            IDM_RESBYTYPE, GRAYED
        MENUITEM "Recursos por conexión",        IDM_RESBYCONN, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "Mostrar dispositivos ocultos", IDM_SHOWHIDDEN
    END
    POPUP "Ayuda"
    BEGIN
        MENUITEM "Acerca de",                    IDM_ABOUT
    END
END


