        MENUITEM SEPARATOR
        MENUITEM "Rejtett eszközök megjelenítése",  IDM_SHOWHIDDEN
    END
    POPUP "Súgó"
    BEGIN
        MENUITEM "Névjegy",                         IDM_ABOUT
    END
END


