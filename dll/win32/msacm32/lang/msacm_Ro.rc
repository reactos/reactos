LANGUAGE LANG_ROMANIAN, SUBLANG_NEUTRAL

DLG_ACMFORMATCHOOSE_ID DIALOGEX  10, 20, 225, 118
STYLE DS_SHELLFONT | DS_MODALFRAME | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION "Selectare sunet"
FONT 8, "MS Shell Dlg"
BEGIN

    LTEXT           "&Nume:", -1, 5, 5, 115, 8, NOT WS_GROUP

    COMBOBOX        IDD_ACMFORMATCHOOSE_CMB_CUSTOM, 5, 15, 135, 60,
                    CBS_DROPDOWNLIST | CBS_SORT | WS_VSCROLL | WS_TABSTOP

    PUSHBUTTON      "&Salvare ca…", IDD_ACMFORMATCHOOSE_BTN_SETNAME, 150, 14, 70, 14
    PUSHBUTTON      "&Eliminare", IDD_ACMFORMATCHOOSE_BTN_DELNAME, 150, 32, 70, 14

    LTEXT           "&Format:", -1, 5, 56, 44, 8, NOT WS_GROUP

    COMBOBOX        IDD_ACMFORMATCHOOSE_CMB_FORMATTAG, 50, 54, 170, 60,
                    CBS_DROPDOWNLIST | CBS_SORT | WS_VSCROLL | WS_TABSTOP

    LTEXT           "&Atribute:", -1, 5, 76, 44, 8, NOT WS_GROUP

    COMBOBOX        IDD_ACMFORMATCHOOSE_CMB_FORMAT, 50, 74, 170, 60,
                    CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP

    PUSHBUTTON      "A&jutor", IDD_ACMFORMATCHOOSE_BTN_HELP, 5, 100, 50, 14
    DEFPUSHBUTTON   "OK", IDOK, 115, 100, 50, 14
    PUSHBUTTON      "Revocare", IDCANCEL, 170, 100, 50, 14

END
