LANGUAGE LANG_ARABIC, SUBLANG_ARABIC_SAUDI_ARABIA

IDC_MAGNIFIER MENU
BEGIN
    POPUP "&ملف"
    BEGIN
        MENUITEM "خ&روج", IDM_EXIT
        MENUITEM "&خيارات", IDM_OPTIONS
        MENUITEM SEPARATOR
        MENUITEM "&حول", IDM_ABOUT
    END
END

IDC_MAGNIFIER ACCELERATORS
BEGIN
