LANGUAGE LANG_BULGARIAN, SUBLANG_DEFAULT

IDC_MAGNIFIER MENU
BEGIN
    POPUP "&Файл"
    BEGIN
        MENUITEM "Из&ход",                       IDM_EXIT
        MENUITEM "На&стройки",                    IDM_OPTIONS
        MENUITEM SEPARATOR
        MENUITEM "&За...",                  IDM_ABOUT
    END
END

IDC_MAGNIFIER ACCELERATORS
BEGIN
    "?",            IDM_ABOUT,              ASCII,  ALT
    "/",            IDM_ABOUT,              ASCII,  ALT
END

