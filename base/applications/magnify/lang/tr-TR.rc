LANGUAGE LANG_TURKISH, SUBLANG_DEFAULT

IDC_MAGNIFIER MENU
BEGIN
    POPUP "&Seçke"
    BEGIN
        MENUITEM "&Çıkış",                     IDM_EXIT
        MENUITEM "&Ayarlar...",                IDM_OPTIONS
        MENUITEM SEPARATOR
        MENUITEM "&Hakkında",                  IDM_ABOUT
    END
END

IDC_MAGNIFIER ACCELERATORS
BEGIN
    "?",            IDM_ABOUT,              ASCII,  ALT
    "/",            IDM_ABOUT,              ASCII,  ALT
END

