    PUSHBUTTON "默认", IDC_DEFAULT_SETTINGS, 8, 245, 60, 14, WS_GROUP | WS_TABSTOP
    DEFPUSHBUTTON "确定", IDOK, 116, 245, 60, 14
    PUSHBUTTON "取消", IDCANCEL, 181, 245, 60, 14
END

