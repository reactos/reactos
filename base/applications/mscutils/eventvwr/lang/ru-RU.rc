CAPTION "Свойства события"
FONT 8, "MS Shell Dlg"
BEGIN
    LTEXT "EvtDetailsCtrl", IDC_STATIC, 0, 0, 266, 215
    PUSHBUTTON "П&омощь", IDHELP, 8, 220, 55, 14
    DEFPUSHBUTTON "&Закрыть", IDOK, 208, 220, 50, 14
END

