CAPTION "Détails de l'événement"
FONT 8, "MS Shell Dlg"
BEGIN
    LTEXT "EvtDetailsCtrl", IDC_STATIC, 0, 0, 266, 215
    PUSHBUTTON "Aide", IDHELP, 8, 220, 50, 14
    DEFPUSHBUTTON "Fermer", IDOK, 208, 220, 50, 14
END

