LANGUAGE LANG_ARABIC, SUBLANG_ARABIC_SAUDI_ARABIA

ID_ACCEL ACCELERATORS
BEGIN
    "^A", CMD_SELECT_ALL
    "^C", CMD_COPY
    "^F", CMD_SEARCH
    "^G", CMD_GOTO
    "H", CMD_REPLACE, VIRTKEY, CONTROL
    "^N", CMD_NEW
    "N", CMD_NEW_WINDOW, VIRTKEY,CONTROL , SHIFT
    "^O", CMD_OPEN
    "^P", CMD_PRINT
    "^S", CMD_SAVE
    "^V", CMD_PASTE
    "^X", CMD_CUT
    "^Z", CMD_UNDO
    VK_F3, CMD_SEARCH_NEXT, VIRTKEY
    VK_F3, CMD_SEARCH_PREV, VIRTKEY, SHIFT
    VK_F5, CMD_TIME_DATE, VIRTKEY
END

MAIN_MENU MENU
BEGIN
    POPUP "&ملف"
    BEGIN
        MENUITEM "&جديد\tCtrl+N", CMD_NEW
        MENUITEM "نافذة &جديدة\tCtrl+Shift+N", CMD_NEW_WINDOW
        MENUITEM "&فتح...\tCtrl+O", CMD_OPEN
        MENUITEM "&حفظ\tCtrl+S", CMD_SAVE
        MENUITEM "حفظ &باسم...", CMD_SAVE_AS
        MENUITEM SEPARATOR
        MENUITEM "إ&عداد الصفحة...", CMD_PAGE_SETUP
        MENUITEM "&طباعة...\tCtrl+P", CMD_PRINT
        MENUITEM SEPARATOR
        MENUITEM "خ&روج", CMD_EXIT
    END
    POPUP "ت&حرير"
    BEGIN
        MENUITEM "&تراجع\tCtrl+Z", CMD_UNDO
        MENUITEM SEPARATOR
        MENUITEM "&قص\tCtrl+X", CMD_CUT
        MENUITEM "&نسخ\tCtrl+C", CMD_COPY
        MENUITEM "ل&صق\tCtrl+V", CMD_PASTE
        MENUITEM "&حذف\tDel", CMD_DELETE
        MENUITEM SEPARATOR
        MENUITEM "&بحث...\tCtrl+F", CMD_SEARCH
        MENUITEM "بحث عن ال&تالي\tF3", CMD_SEARCH_NEXT
        MENUITEM "اس&تبدال...\tCtrl+H", CMD_REPLACE
        MENUITEM "ا&نتقال إلى...\tCtrl+G", CMD_GOTO
        MENUITEM SEPARATOR
        MENUITEM "تحديد ال&كل\tCtrl+A", CMD_SELECT_ALL
        MENUITEM "الوقت/ال&تاريخ\tF5", CMD_TIME_DATE
    END
    POPUP "ت&نسيق"
    BEGIN
        MENUITEM "التفاف ال&كلمات", CMD_WRAP
        MENUITEM "ال&خط...", CMD_FONT
    END
    POPUP "&عرض"
    BEGIN
        MENUITEM "شريط ال&حالة", CMD_STATUSBAR
    END
    POPUP "م&ساعدة"
    BEGIN
        MENUITEM "عرض ال&مساعدة", CMD_HELP_CONTENTS
        MENUITEM SEPARATOR
        MENUITEM "&حول المفكرة", CMD_HELP_ABOUT_NOTEPAD
    END
END

