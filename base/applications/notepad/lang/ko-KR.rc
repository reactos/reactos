    DEFPUSHBUTTON "확인", IDOK, 198, 170, 50, 14, BS_PUSHBUTTON
    PUSHBUTTON "취소", IDCANCEL, 254, 170, 50, 14
    PUSHBUTTON "프린터(&R)...", psh3, 310, 170, 50, 14
END

