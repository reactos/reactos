LANGUAGE LANG_ARABIC, SUBLANG_ARABIC_SAUDI_ARABIA

IDB_FONTSTOOLBAR BITMAP "img/fontstoolbar.bmp"

ID_MENU MENU
BEGIN
    POPUP "&ملف"
    BEGIN
        MENUITEM "&جديد\tCtrl+N", IDM_FILENEW
        MENUITEM "&فتح...\tCtrl+O", IDM_FILEOPEN
        MENUITEM "&حفظ\tCtrl+S", IDM_FILESAVE
        MENUITEM "حفظ &باسم...", IDM_FILESAVEAS
        MENUITEM SEPARATOR
        MENUITEM "من الماسح الضوئي أو الكاميرا...", IDM_FILEFROMSCANNERORCAMERA
        MENUITEM SEPARATOR
        MENUITEM "عرض الصفحة", IDM_FILEPAGEVIEW
        MENUITEM "إعداد الصفحة...", IDM_FILEPAGESETUP
        MENUITEM "&طباعة...\tCtrl+P", IDM_FILEPRINT
        MENUITEM SEPARATOR
        MENUITEM "إرسال...", IDM_FILESEND
        MENUITEM SEPARATOR
        MENUITEM "تعيين كخلفية (مكررة)", IDM_FILEASWALLPAPERPLANE
        MENUITEM "تعيين كخلفية (مركزة)", IDM_FILEASWALLPAPERCENTERED
        MENUITEM "تعيين كخلفية (ممتدة)", IDM_FILEASWALLPAPERSTRETCHED
        MENUITEM SEPARATOR
        MENUITEM "آخر ملف مستخدم", IDM_FILEMOSTRECENTLYUSEDFILE, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "&خروج\tAlt+F4", IDM_FILEEXIT
    END
    POPUP "&تحرير"
    BEGIN
        MENUITEM "&تراجع\tCtrl+Z", IDM_EDITUNDO
        MENUITEM "إعا&دة\tCtrl+Y", IDM_EDITREDO
        MENUITEM SEPARATOR
        MENUITEM "&قص\tCtrl+X", IDM_EDITCUT
        MENUITEM "&نسخ\tCtrl+C", IDM_EDITCOPY
        MENUITEM "&لصق\tCtrl+V", IDM_EDITPASTE
        MENUITEM SEPARATOR
        MENUITEM "قص التحدي&د", IDM_CROPSELECTION
        MENUITEM "ح&ذف التحديد\tDel", IDM_EDITDELETESELECTION
        MENUITEM "ع&كس التحديد", IDM_EDITINVERTSELECTION
        MENUITEM "تحديد ال&كل\tCtrl+A", IDM_EDITSELECTALL
        MENUITEM SEPARATOR
        MENUITEM "نسخ إ&لى...", IDM_EDITCOPYTO
        MENUITEM "لصق م&ن...", IDM_EDITPASTEFROM
    END
    POPUP "&عرض"
    BEGIN
        MENUITEM "&صندوق الأدوات\tCtrl+T", IDM_VIEWTOOLBOX, CHECKED
        MENUITEM "لو&نة الألوان\tCtrl+L", IDM_VIEWCOLORPALETTE, CHECKED
        MENUITEM "شريط الح&الة", IDM_VIEWSTATUSBAR, CHECKED
        MENUITEM "شريط أدوات الن&ص", IDM_FORMATICONBAR, CHECKED, GRAYED
        MENUITEM SEPARATOR
        POPUP "&تكبير/تصغير"
        BEGIN
            POPUP "&محدد من قبل المستخدم"
            BEGIN
                MENUITEM "12.5%", IDM_VIEWZOOM125
                MENUITEM "25%", IDM_VIEWZOOM25
                MENUITEM "50%", IDM_VIEWZOOM50
                MENUITEM "100%", IDM_VIEWZOOM100
                MENUITEM "200%", IDM_VIEWZOOM200
                MENUITEM "400%", IDM_VIEWZOOM400
                MENUITEM "800%", IDM_VIEWZOOM800
            END
            MENUITEM SEPARATOR
            MENUITEM "إظهار الشبك&ة\tCtrl+G", IDM_VIEWSHOWGRID
            MENUITEM "إظهار صورة &مصغرة", IDM_VIEWSHOWMINIATURE
        END
        MENUITEM "&ملء الشاشة\tCtrl+F", IDM_VIEWFULLSCREEN
    END
    POPUP "&صورة"
    BEGIN
        MENUITEM "تدوير/ع&كس...\tCtrl+R", IDM_IMAGEROTATEMIRROR
        MENUITEM "&تمديد/إمالة...\tCtrl+W", IDM_IMAGESTRETCHSKEW
        MENUITEM "ق&ص", IDM_IMAGECROP
        MENUITEM "ع&كس الألوان\tCtrl+I", IDM_IMAGEINVERTCOLORS
        MENUITEM "خصائ&ص...\tCtrl+E", IDM_IMAGEATTRIBUTES
        MENUITEM "&مسح الصورة\tCtrl+Shft+N", IDM_IMAGEDELETEIMAGE
        MENUITEM "رسم &غير شفاف", IDM_IMAGEDRAWOPAQUE
    END
    POPUP "أل&وان"
    BEGIN
        MENUITEM "&تحرير اللوحة...", IDM_COLORSEDITPALETTE
        MENUITEM SEPARATOR
        MENUITEM "لوحة حديثة", IDM_COLORSMODERNPALETTE
        MENUITEM "لوحة قديمة", IDM_COLORSOLDPALETTE
    END
    POPUP "&مساعدة"
    BEGIN
        MENUITEM "مواضيع المساعدة", IDM_HELPHELPTOPICS
        MENUITEM SEPARATOR
        MENUITEM "&معلومات", IDM_HELPINFO
    END
END

ID_POPUPMENU MENU
BEGIN
    POPUP "قائمة منبثقة #0"
    BEGIN
        MENUITEM "ق&ص\tCtrl+X", IDM_EDITCUT
        MENUITEM "&نسخ\tCtrl+C", IDM_EDITCOPY
        MENUITEM "&لصق\tCtrl+V", IDM_EDITPASTE
        MENUITEM SEPARATOR
        MENUITEM "قص التحدي&د", IDM_CROPSELECTION
        MENUITEM "ح&ذف التحديد\tDel", IDM_EDITDELETESELECTION
        MENUITEM "تحديد ال&كل\tCtrl+A", IDM_EDITSELECTALL
        MENUITEM SEPARATOR
        MENUITEM "نسخ إ&لى...", IDM_EDITCOPYTO
        MENUITEM "لصق م&ن...", IDM_EDITPASTEFROM
        MENUITEM SEPARATOR
        MENUITEM "تدوير/ع&كس...\tCtrl+R", IDM_IMAGEROTATEMIRROR
        MENUITEM "&تمديد/إمالة...\tCtrl+W", IDM_IMAGESTRETCHSKEW
        MENUITEM "ع&كس الألوان\tCtrl+I", IDM_IMAGEINVERTCOLORS
    END
END

ID_ACCELERATORS ACCELERATORS
BEGIN
    "^N", IDM_FILENEW
    "^O", IDM_FILEOPEN
    "^S", IDM_FILESAVE
    "^Z", IDM_EDITUNDO
    "^Y", IDM_EDITREDO
    "^X", IDM_EDITCUT
    "^C", IDM_EDITCOPY
    "^V", IDM_EDITPASTE
    VK_DELETE, IDM_EDITDELETESELECTION, VIRTKEY
    "^A", IDM_EDITSELECTALL
    "^T", IDM_VIEWTOOLBOX
    "^L", IDM_VIEWCOLORPALETTE
    "^G", IDM_VIEWSHOWGRID
    "^F", IDM_VIEWFULLSCREEN
    "^R", IDM_IMAGEROTATEMIRROR
    "^W", IDM_IMAGESTRETCHSKEW
    "^I", IDM_IMAGEINVERTCOLORS
    "^E", IDM_IMAGEATTRIBUTES
    "N", IDM_IMAGEDELETEIMAGE, CONTROL, SHIFT, VIRTKEY
    VK_ADD, IDM_CTRL_PLUS, CONTROL, VIRTKEY
    VK_SUBTRACT, IDM_CTRL_MINUS, CONTROL, VIRTKEY
    VK_OEM_PLUS, IDM_CTRL_PLUS, CONTROL, VIRTKEY
    VK_OEM_MINUS, IDM_CTRL_MINUS, CONTROL, VIRTKEY
END

