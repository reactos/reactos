LANGUAGE LANG_ARABIC, SUBLANG_ARABIC_SAUDI_ARABIA

IDB_FONTSTOOLBAR BITMAP "img/fontstoolbar.bmp"

ID_MENU MENU
BEGIN
    POPUP "&ملف"
    BEGIN
        MENUITEM "&جديد\tCtrl+N", IDM_FILENEW
        MENUITEM "&فتح...\tCtrl+O", IDM_FILEOPEN
        MENUITEM "&حفظ\tCtrl+S", IDM_FILESAVE
        MENUITEM "حفظ &باسم...", IDM_FILESAVEAS
        MENUITEM SEPARATOR
        MENUITEM "من الماسح الضوئي أو الكاميرا...", IDM_FILEFROMSCANNERORCAMERA
        MENUITEM SEPARATOR
        MENUITEM "عرض الصفحة", IDM_FILEPAGEVIEW
        MENUITEM "إعداد الصفحة...", IDM_FILEPAGESETUP
        MENUITEM "&طباعة...\tCtrl+P", IDM_FILEPRINT
        MENUITEM SEPARATOR
        MENUITEM "إرسال...", IDM_FILESEND
        MENUITEM SEPARATOR
        MENUITEM "تعيين كخلفية (متكرر)", IDM_FILEASWALLPAPERPLANE
        MENUITEM "تعيين كخلفية (وسط)", IDM_FILEASWALLPAPERCENTERED
        MENUITEM "تعيين كخلفية (ممتد)", IDM_FILEASWALLPAPERSTRETCHED
        MENUITEM SEPARATOR
        MENUITEM "الملف المستخدم مؤخراً", IDM_FILEMOSTRECENTLYUSEDFILE, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "خ&روج\tAlt+F4", IDM_FILEEXIT
    END
    POPUP "ت&حرير"
    BEGIN
        MENUITEM "ت&راجع\tCtrl+Z", IDM_EDITUNDO
        MENUITEM "إ&عادة\tCtrl+Y", IDM_EDITREDO
        MENUITEM SEPARATOR
        MENUITEM "&قص\tCtrl+X", IDM_EDITCUT
        MENUITEM "&نسخ\tCtrl+C", IDM_EDITCOPY
        MENUITEM "&لصق\tCtrl+V", IDM_EDITPASTE
        MENUITEM SEPARATOR
        MENUITEM "قص التحديد", IDM_CROPSELECTION
        MENUITEM "ح&ذف التحديد\tDel", IDM_EDITDELETESELECTION
        MENUITEM "عكس التحديد", IDM_EDITINVERTSELECTION
        MENUITEM "تحديد ال&كل\tCtrl+A", IDM_EDITSELECTALL
        MENUITEM SEPARATOR
        MENUITEM "نسخ إ&لى...", IDM_EDITCOPYTO
        MENUITEM "لصق &من...", IDM_EDITPASTEFROM
    END
    POPUP "&عرض"
    BEGIN
        MENUITEM "صندوق الأ&دوات\tCtrl+T", IDM_VIEWTOOLBOX, CHECKED
        MENUITEM "لوحة الأ&لوان\tCtrl+L", IDM_VIEWCOLORPALETTE, CHECKED
        MENUITEM "شريط ال&حالة", IDM_VIEWSTATUSBAR, CHECKED
        MENUITEM "شريط أدوات ال&نص", IDM_FORMATICONBAR, CHECKED, GRAYED
        MENUITEM SEPARATOR
        POPUP "ت&كبير"
        BEGIN
            POPUP "تحديد المست&خدم"
            BEGIN
                MENUITEM "12.5%", IDM_VIEWZOOM125
                MENUITEM "25%", IDM_VIEWZOOM25
                MENUITEM "50%", IDM_VIEWZOOM50
                MENUITEM "100%", IDM_VIEWZOOM100
                MENUITEM "200%", IDM_VIEWZOOM200
                MENUITEM "400%", IDM_VIEWZOOM400
                MENUITEM "800%", IDM_VIEWZOOM800
            END
            MENUITEM SEPARATOR
            MENUITEM "إظهار ال&شبكة\tCtrl+G", IDM_VIEWSHOWGRID
            MENUITEM "إظهار المصغرة", IDM_VIEWSHOWMINIATURE
        END
        MENUITEM "ملء الشاشة\tCtrl+F", IDM_VIEWFULLSCREEN
    END
    POPUP "ال&صورة"
    BEGIN
        MENUITEM "تدوير/&عكس...\tCtrl+R", IDM_IMAGEROTATEMIRROR
        MENUITEM "ت&مديد/إمالة...\tCtrl+W", IDM_IMAGESTRETCHSKEW
        MENUITEM "&قص", IDM_IMAGECROP
        MENUITEM "عكس الأ&لوان\tCtrl+I", IDM_IMAGEINVERTCOLORS
        MENUITEM "ال&خصائص...\tCtrl+E", IDM_IMAGEATTRIBUTES
        MENUITEM "مسح الصورة\tCtrl+Shft+N", IDM_IMAGEDELETEIMAGE
        MENUITEM "رسم غير شفاف", IDM_IMAGEDRAWOPAQUE
    END
    POPUP "الأ&لوان"
    BEGIN
        MENUITEM "تحرير لوحة الألوان...", IDM_COLORSEDITPALETTE
        MENUITEM SEPARATOR
        MENUITEM "لوحة الألوان الحديثة", IDM_COLORSMODERNPALETTE
        MENUITEM "لوحة الألوان القديمة", IDM_COLORSOLDPALETTE
    END
    POPUP "م&ساعدة"
    BEGIN
        MENUITEM "مواضيع المساعدة", IDM_HELPHELPTOPICS
        MENUITEM SEPARATOR
        MENUITEM "حول", IDM_HELPINFO
    END
END

ID_POPUPMENU MENU
BEGIN
    POPUP "Popup #0"
    BEGIN
        MENUITEM "&قص\tCtrl+X", IDM_EDITCUT
        MENUITEM "&نسخ\tCtrl+C", IDM_EDITCOPY
        MENUITEM "&لصق\tCtrl+V", IDM_EDITPASTE
        MENUITEM SEPARATOR
        MENUITEM "قص التحديد", IDM_CROPSELECTION
        MENUITEM "ح&ذف التحديد\tDel", IDM_EDITDELETESELECTION
        MENUITEM "تحديد ال&كل\tCtrl+A", IDM_EDITSELECTALL
        MENUITEM SEPARATOR
        MENUITEM "نسخ إ&لى...", IDM_EDITCOPYTO
        MENUITEM "لصق &من...", IDM_EDITPASTEFROM
        MENUITEM SEPARATOR
        MENUITEM "تدوير/&عكس...\tCtrl+R", IDM_IMAGEROTATEMIRROR
        MENUITEM "ت&مديد/إمالة...\tCtrl+W", IDM_IMAGESTRETCHSKEW
        MENUITEM "عكس الأ&لوان\tCtrl+I", IDM_IMAGEINVERTCOLORS
    END
END

ID_ACCELERATORS ACCELERATORS
BEGIN
    "^N", IDM_FILENEW
    "^O", IDM_FILEOPEN
    "^S", IDM_FILESAVE
    "^Z", IDM_EDITUNDO
    "^Y", IDM_EDITREDO
    "^X", IDM_EDITCUT
    "^C", IDM_EDITCOPY
    "^V", IDM_EDITPASTE
    VK_DELETE, IDM_EDITDELETESELECTION, VIRTKEY
    "^A", IDM_EDITSELECTALL
    "^T", IDM_VIEWTOOLBOX
    "^L", IDM_VIEWCOLORPALETTE
    "^G", IDM_VIEWSHOWGRID
    "^F", IDM_VIEWFULLSCREEN
    "^R", IDM_IMAGEROTATEMIRROR
    "^W", IDM_IMAGESTRETCHSKEW
    "^I", IDM_IMAGEINVERTCOLORS
    "^E", IDM_IMAGEATTRIBUTES
    "N", IDM_IMAGEDELETEIMAGE, CONTROL, SHIFT, VIRTKEY
    VK_ADD, IDM_CTRL_PLUS, CONTROL, VIRTKEY
    VK_SUBTRACT, IDM_CTRL_MINUS, CONTROL, VIRTKEY
    VK_OEM_PLUS, IDM_CTRL_PLUS, CONTROL, VIRTKEY
    VK_OEM_MINUS, IDM_CTRL_MINUS, CONTROL, VIRTKEY
END

