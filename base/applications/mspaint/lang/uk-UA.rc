    LTEXT "Розмір файлу:", IDD_ATTRIBUTESTEXT4, 10, 15, 60, 10
    LTEXT "Розширення:", IDD_ATTRIBUTESTEXT5, 10, 25, 60, 10
    LTEXT "Недоступно", IDD_ATTRIBUTESTEXT6, 60, 5, 90, 10
