LANGUAGE LANG_CROATIAN, SUBLANG_CROATIAN_CROATIA

IDB_FONTSTOOLBAR BITMAP "img/fontstoolbar.bmp"

ID_MENU MENU
BEGIN
    POPUP "&Datoteka"
    BEGIN
        MENUITEM "&Novo\tCtrl+N", IDM_FILENEW
        MENUITEM "&Otvori...\tCtrl+O", IDM_FILEOPEN
        MENUITEM "&Spremi\tCtrl+S", IDM_FILESAVE
        MENUITEM "Spremi &kao...", IDM_FILESAVEAS
        MENUITEM SEPARATOR
        MENUITEM "Iz &skenera ili kamere...", IDM_FILEFROMSCANNERORCAMERA
        MENUITEM SEPARATOR
        MENUITEM "Pregled stranice", IDM_FILEPAGEVIEW
        MENUITEM "Postavljanje stranice...", IDM_FILEPAGESETUP
        MENUITEM "&Ispis...\tCtrl+P", IDM_FILEPRINT
        MENUITEM SEPARATOR
        MENUITEM "Pošalji...", IDM_FILESEND
        MENUITEM SEPARATOR
        MENUITEM "Postavi kao pozadinu (popločano)", IDM_FILEASWALLPAPERPLANE
        MENUITEM "Postavi kao pozadinu (centrirano)", IDM_FILEASWALLPAPERCENTERED
        MENUITEM "Postavi kao pozadinu (razvučeno)", IDM_FILEASWALLPAPERSTRETCHED
        MENUITEM SEPARATOR
        MENUITEM "Nedavno korištene datoteke", IDM_FILEMOSTRECENTLYUSEDFILE, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "&Izlaz\tAlt+F4", IDM_FILEEXIT
    END
    POPUP "&Uređivanje"
    BEGIN
        MENUITEM "&Poništi\tCtrl+Z", IDM_EDITUNDO
        MENUITEM "&Ponovi\tCtrl+Y", IDM_EDITREDO
        MENUITEM SEPARATOR
        MENUITEM "Izre&ži\tCtrl+X", IDM_EDITCUT
        MENUITEM "&Kopiraj\tCtrl+C", IDM_EDITCOPY
        MENUITEM "&Zalijepi\tCtrl+V", IDM_EDITPASTE
        MENUITEM SEPARATOR
        MENUITEM "Izreži od&abir", IDM_CROPSELECTION
        MENUITEM "Ob&riši odabir\tDel", IDM_EDITDELETESELECTION
        MENUITEM "O&brni odabir", IDM_EDITINVERTSELECTION
        MENUITEM "Odaberi s&ve\tCtrl+A", IDM_EDITSELECTALL
        MENUITEM SEPARATOR
        MENUITEM "Kopiraj u...", IDM_EDITCOPYTO
        MENUITEM "Zalijepi iz...", IDM_EDITPASTEFROM
    END
    POPUP "&Prikaz"
    BEGIN
        MENUITEM "&Alatna traka\tCtrl+T", IDM_VIEWTOOLBOX, CHECKED
        MENUITEM "&Paleta boja\tCtrl+L", IDM_VIEWCOLORPALETTE, CHECKED
        MENUITEM "&Traka stanja", IDM_VIEWSTATUSBAR, CHECKED
        MENUITEM "Traka za &tekst", IDM_FORMATICONBAR, CHECKED, GRAYED
        MENUITEM SEPARATOR
        POPUP "&Zum"
        BEGIN
            POPUP "&Prilagođeni"
            BEGIN
                MENUITEM "12.5%", IDM_VIEWZOOM125
                MENUITEM "25%", IDM_VIEWZOOM25
                MENUITEM "50%", IDM_VIEWZOOM50
                MENUITEM "100%", IDM_VIEWZOOM100
                MENUITEM "200%", IDM_VIEWZOOM200
                MENUITEM "400%", IDM_VIEWZOOM400
                MENUITEM "800%", IDM_VIEWZOOM800
            END
            MENUITEM SEPARATOR
            MENUITEM "Prikaži &mrežu\tCtrl+G", IDM_VIEWSHOWGRID
            MENUITEM "Prikaži &minijaturu", IDM_VIEWSHOWMINIATURE
        END
        MENUITEM "&Cijeli zaslon\tCtrl+F", IDM_VIEWFULLSCREEN
    END
    POPUP "&Slika"
    BEGIN
        MENUITEM "Rotiraj i &zrcali...\tCtrl+R", IDM_IMAGEROTATEMIRROR
        MENUITEM "&Promijeni veličinu/iskrivljenje...\tCtrl+W", IDM_IMAGESTRETCHSKEW
        MENUITEM "I&zreži", IDM_IMAGECROP
        MENUITEM "O&brni boje\tCtrl+I", IDM_IMAGEINVERTCOLORS
        MENUITEM "&Svojstva...\tCtrl+E", IDM_IMAGEATTRIBUTES
        MENUITEM "&Obriši sliku\tCtrl+Shift+N", IDM_IMAGEDELETEIMAGE
        MENUITEM "Nacrtaj &neprozirno", IDM_IMAGEDRAWOPAQUE
    END
    POPUP "&Boje"
    BEGIN
        MENUITEM "&Uredi paletu...", IDM_COLORSEDITPALETTE
        MENUITEM SEPARATOR
        MENUITEM "Nova paleta", IDM_COLORSMODERNPALETTE
        MENUITEM "Klasična paleta", IDM_COLORSOLDPALETTE
    END
    POPUP "&Pomoć"
    BEGIN
        MENUITEM "&Sadržaj pomoći", IDM_HELPHELPTOPICS
        MENUITEM SEPARATOR
        MENUITEM "&O programu", IDM_HELPINFO
    END
END

ID_POPUPMENU MENU
BEGIN
    POPUP "Popup #0"
    BEGIN
        MENUITEM "Izre&ži\tCtrl+X", IDM_EDITCUT
        MENUITEM "&Kopiraj\tCtrl+C", IDM_EDITCOPY
        MENUITEM "&Zalijepi\tCtrl+V", IDM_EDITPASTE
        MENUITEM SEPARATOR
        MENUITEM "Izreži &odabir", IDM_CROPSELECTION
        MENUITEM "Ob&riši odabir\tDel", IDM_EDITDELETESELECTION
        MENUITEM "Odaberi &sve\tCtrl+A", IDM_EDITSELECTALL
        MENUITEM SEPARATOR
        MENUITEM "&Kopiraj u...", IDM_EDITCOPYTO
        MENUITEM "Zalijepi &iz...", IDM_EDITPASTEFROM
        MENUITEM SEPARATOR
        MENUITEM "Rotiraj i &zrcali...\tCtrl+R", IDM_IMAGEROTATEMIRROR
        MENUITEM "&Promijeni veličinu/iskrivljenje...\tCtrl+W", IDM_IMAGESTRETCHSKEW
        MENUITEM "O&brni boje\tCtrl+I", IDM_IMAGEINVERTCOLORS
    END
END

ID_ACCELERATORS ACCELERATORS
BEGIN
    "^N", IDM_FILENEW
    "^O", IDM_FILEOPEN
    "^S", IDM_FILESAVE
    "^Z", IDM_EDITUNDO
    "^Y", IDM_EDITREDO
    "^X", IDM_EDITCUT
    "^C", IDM_EDITCOPY
    "^V", IDM_EDITPASTE
    VK_DELETE, IDM_EDITDELETESELECTION, VIRTKEY
    "^A", IDM_EDITSELECTALL
    "^T", IDM_VIEWTOOLBOX
    "^L", IDM_VIEWCOLORPALETTE
    "^G", IDM_VIEWSHOWGRID
    "^F", IDM_VIEWFULLSCREEN
    "^R", IDM_IMAGEROTATEMIRROR
    "^W", IDM_IMAGESTRETCHSKEW
    "^I", IDM_IMAGEINVERTCOLORS
    "^E", IDM_IMAGEATTRIBUTES
    "N", IDM_IMAGEDELETEIMAGE, CONTROL, SHIFT, VIRTKEY
    VK_ADD, IDM_CTRL_PLUS, CONTROL, VIRTKEY
    VK_SUBTRACT, IDM_CTRL_MINUS, CONTROL, VIRTKEY
    VK_OEM_PLUS, IDM_CTRL_PLUS, CONTROL, VIRTKEY
    VK_OEM_MINUS, IDM_CTRL_MINUS, CONTROL, VIRTKEY
END

