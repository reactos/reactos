        MENUITEM SEPARATOR
        MENUITEM "З&найти...\tCtrl+F", ID_EDIT_FIND
        MENUITEM "Зна&йти далі\tF3", ID_EDIT_FINDNEXT
    END
    POPUP "&Вигляд"
    BEGIN
        MENUITEM "&Рядок стану", ID_VIEW_STATUSBAR
        MENUITEM SEPARATOR
        MENUITEM "Роз&ділити", ID_VIEW_SPLIT
        MENUITEM SEPARATOR
