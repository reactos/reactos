/* Command-line strings */
STRINGTABLE
BEGIN
    STRING_USAGE "الاستخدام:\n\
  regedit [خيارات] [اسم_الملف] [مفتاح_السجل]\n\n\
الخيارات:\n\
  [بدون خيار]    تشغيل النسخة الرسومية من البرنامج.\n\
  /L:system.dat  موقع ملف system.dat المراد تعديله.\n\
                 متوافق مع أي مفتاح آخر. يتم تجاهله.\n\
  /R:user.dat    موقع ملف user.dat المراد تعديله.\n\
                 متوافق مع أي مفتاح آخر. يتم تجاهله.\n\
  /C             استيراد محتويات ملف السجل.\n\
  /D             حذف مفتاح سجل محدد.\n\
  /E             تصدير محتويات مفتاح سجل محدد إلى ملف.\n\
                 إذا لم يتم تحديد مفتاح، يتم تصدير السجل بأكمله.\n\
  /S             الوضع الصامت. لن يتم عرض أي رسائل.\n\
  /V             تشغيل الواجهة الرسومية في الوضع المتقدم. يتم تجاهله.\n\
  /?             عرض هذه المعلومات والخروج.\n\
  [اسم_الملف]     موقع الملف الذي يحتوي على معلومات السجل المراد\n\
                 استيرادها. عند استخدامه مع [/E]، يحدد هذا الخيار موقع\n\
                 الملف حيث سيتم تصدير معلومات السجل.\n\
  [مفتاح_السجل]      مفتاح السجل المراد تعديله.\n\
\n\
أمثلة الاستخدام:\n\
  regedit ""import.reg""\n\
  regedit /E ""export.reg"" ""HKEY_CURRENT_USER\\Console""\n\
  regedit /D ""HKEY_LOCAL_MACHINE\\Error\\Path""\n"
    STRING_INVALID_SWITCH "regedit: مفتاح غير صالح أو غير معروف [%1]\n"
    STRING_HELP "اكتب ""regedit /?"" للمساعدة.\n"
    STRING_NO_FILENAME "regedit: لم يتم تحديد اسم الملف.\n"
    STRING_NO_REG_KEY "regedit: لم يتم تحديد مفتاح السجل للحذف.\n"
    STRING_FILE_NOT_FOUND "regedit: لم يتم العثور على الملف '%1'.\n"
    STRING_CANNOT_OPEN_FILE "regedit: تعذر فتح الملف '%1'.\n"
    STRING_UNHANDLED_ACTION "regedit: إجراء غير معالج.\n"
    STRING_OUT_OF_MEMORY "regedit: نفدت الذاكرة! (%1!S!، السطر %2!u!)\n"
    STRING_INVALID_HEX "regedit: قيمة سداسية عشرية غير صالحة.\n"
    STRING_CSV_HEX_ERROR "regedit: تعذر تحويل البيانات السداسية العشرية. تم العثور على قيمة غير صالحة في '%1'.\n"
    STRING_ESCAPE_SEQUENCE "regedit: تسلسل هروب غير معروف [\\%1!c!]\n"
    STRING_UNKNOWN_DATA_FORMAT "regedit: نوع بيانات السجل غير مدعوم [0x%1!x!]\n"
    STRING_UNEXPECTED_EOL "regedit: نهاية غير متوقعة للسطر في '%1'.\n"
    STRING_UNRECOGNIZED_LINE "regedit: السطر '%1' غير معروف.\n"
    STRING_SETVALUE_FAILED "regedit: تعذر إضافة قيمة السجل '%1' إلى '%2'.\n"
    STRING_OPEN_KEY_FAILED "regedit: تعذر فتح مفتاح السجل '%1'.\n"
    STRING_UNSUPPORTED_TYPE "regedit: نوع بيانات السجل غير مدعوم [0x%1!x!] في '%2'.\n"
    STRING_EXPORT_AS_BINARY "regedit: سيتم تصدير قيمة السجل '%1' كبيانات ثنائية.\n"
    STRING_INVALID_SYSTEM_KEY "regedit: مفتاح نظام غير صالح [%1]\n"
    STRING_REG_KEY_NOT_FOUND "regedit: تعذر تصدير '%1'. لم يتم العثور على مفتاح السجل المحدد.\n"
    STRING_DELETE_FAILED "regedit: تعذر حذف مفتاح السجل '%1'.\n"
    STRING_UNKNOWN_TYPE "نوع غير معروف"
    STRING_INVALID_LINE_SYNTAX, "regedit: يحتوي السطر على بناء جملة غير صالح.\n"
END

/* Menus */

IDR_REGEDIT_MENU MENU
BEGIN
    POPUP "&ملف"
    BEGIN
