/* Command-line strings */
STRINGTABLE
BEGIN
    STRING_USAGE "طريقة الاستخدام:\n\
  regedit [خيارات] [اسم_الملف] [مفتاح_التسجيل]\n\n\
الخيارات:\n\
  [بدون خيار]    تشغيل النسخة الرسومية من البرنامج.\n\
  /L:system.dat  موقع ملف system.dat المراد تعديله.\n\
                 متوافق مع أي مفتاح آخر. يتم تجاهله.\n\
  /R:user.dat    موقع ملف user.dat المراد تعديله.\n\
                 متوافق مع أي مفتاح آخر. يتم تجاهله.\n\
  /C             استيراد محتويات ملف التسجيل.\n\
  /D             حذف مفتاح تسجيل محدد.\n\
  /E             تصدير محتويات مفتاح تسجيل محدد إلى ملف.\n\
                 إذا لم يتم تحديد مفتاح، يتم تصدير كامل التسجيل.\n\
  /S             الوضع الصامت. لن يتم عرض أي رسائل.\n\
  /V             تشغيل الواجهة الرسومية في الوضع المتقدم. يتم تجاهله.\n\
  /?             عرض هذه المعلومات والخروج.\n\
  [اسم_الملف]     موقع الملف الذي يحتوي على معلومات التسجيل المراد\n\
                 استيرادها. عند استخدامه مع [/E]، يحدد هذا الخيار\n\
                 موقع الملف حيث سيتم تصدير معلومات التسجيل.\n\
  [مفتاح_التسجيل]  مفتاح التسجيل المراد تعديله.\n\
\n\
أمثلة للاستخدام:\n\
  regedit ""import.reg""\n\
  regedit /E ""export.reg"" ""HKEY_CURRENT_USER\\Console""\n\
  regedit /D ""HKEY_LOCAL_MACHINE\\Error\\Path""\n"
    STRING_INVALID_SWITCH "regedit: مفتاح غير صالح أو غير معروف [%1]\n"
    STRING_HELP "اكتب ""regedit /?"" للمساعدة.\n"
    STRING_NO_FILENAME "regedit: لم يتم تحديد اسم ملف.\n"
    STRING_NO_REG_KEY "regedit: لم يتم تحديد مفتاح تسجيل للحذف.\n"
    STRING_FILE_NOT_FOUND "regedit: الملف '%1' غير موجود.\n"
    STRING_CANNOT_OPEN_FILE "regedit: تعذر فتح الملف '%1'.\n"
    STRING_UNHANDLED_ACTION "regedit: إجراء غير معالج.\n"
    STRING_OUT_OF_MEMORY "regedit: نفاد الذاكرة! (%1!S!, سطر %2!u!)\n"
    STRING_INVALID_HEX "regedit: قيمة ست عشرية غير صالحة.\n"
    STRING_CSV_HEX_ERROR "regedit: تعذر تحويل البيانات الست عشرية. تمت مواجهة قيمة غير صالحة عند '%1'.\n"
    STRING_ESCAPE_SEQUENCE "regedit: تسلسل هروب غير معروف [\\%1!c!]\n"
    STRING_UNKNOWN_DATA_FORMAT "regedit: نوع بيانات تسجيل غير مدعوم [0x%1!x!]\n"
    STRING_UNEXPECTED_EOL "regedit: نهاية سطر غير متوقعة في '%1'.\n"
    STRING_UNRECOGNIZED_LINE "regedit: السطر '%1' غير معروف.\n"
    STRING_SETVALUE_FAILED "regedit: تعذر إضافة قيمة التسجيل '%1' إلى '%2'.\n"
    STRING_OPEN_KEY_FAILED "regedit: تعذر فتح مفتاح التسجيل '%1'.\n"
    STRING_UNSUPPORTED_TYPE "regedit: تمت مواجهة نوع بيانات تسجيل غير مدعوم [0x%1!x!] في '%2'.\n"
    STRING_EXPORT_AS_BINARY "regedit: سيتم تصدير قيمة التسجيل '%1' كبيانات ثنائية.\n"
    STRING_INVALID_SYSTEM_KEY "regedit: مفتاح نظام غير صالح [%1]\n"
    STRING_REG_KEY_NOT_FOUND "regedit: تعذر تصدير '%1'. لم يتم العثور على مفتاح التسجيل المحدد.\n"
    STRING_DELETE_FAILED "regedit: تعذر حذف مفتاح التسجيل '%1'.\n"
    STRING_UNKNOWN_TYPE "نوع غير معروف"
    STRING_INVALID_LINE_SYNTAX, "regedit: السطر يحتوي على صيغة غير صالحة.\n"
END

/* Menus */

IDR_REGEDIT_MENU MENU
BEGIN
    POPUP "&ملف"
    BEGIN
        MENUITEM "&استيراد...", ID_REGISTRY_IMPORTREGISTRYFILE
        MENUITEM "&تصدير...", ID_REGISTRY_EXPORTREGISTRYFILE
        MENUITEM SEPARATOR
        MENUITEM "&تحميل خلية...", ID_REGISTRY_LOADHIVE, GRAYED
        MENUITEM "&إلغاء تحميل خلية...", ID_REGISTRY_UNLOADHIVE, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "&الاتصال بتسجيل الشبكة...", ID_REGISTRY_CONNECTNETWORKREGISTRY
        MENUITEM "&قطع الاتصال بتسجيل الشبكة...", ID_REGISTRY_DISCONNECTNETWORKREGISTRY, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "&طباعة...\tCtrl+P", ID_REGISTRY_PRINT, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "&خروج", ID_REGISTRY_EXIT
    END
    POPUP "&تحرير"
    BEGIN
        MENUITEM "&تعديل", ID_EDIT_MODIFY
        MENUITEM SEPARATOR
        POPUP "&جديد"
        BEGIN
            MENUITEM "&مفتاح", ID_EDIT_NEW_KEY
            MENUITEM SEPARATOR
            MENUITEM "قيمة &سلسلة", ID_EDIT_NEW_STRINGVALUE
            MENUITEM "قيمة &ثنائية", ID_EDIT_NEW_BINARYVALUE
            MENUITEM "قيمة &DWORD", ID_EDIT_NEW_DWORDVALUE
            MENUITEM "قيمة &متعددة السلاسل", ID_EDIT_NEW_MULTISTRINGVALUE
            MENUITEM "قيمة سلسلة &قابلة للتوسيع", ID_EDIT_NEW_EXPANDABLESTRINGVALUE
        END
        MENUITEM SEPARATOR
        MENUITEM "&أذونات...", ID_EDIT_PERMISSIONS
        MENUITEM SEPARATOR
        MENUITEM "&حذف\tDel", ID_EDIT_DELETE
        MENUITEM "&إعادة تسمية", ID_EDIT_RENAME
        MENUITEM SEPARATOR
        MENUITEM "&نسخ اسم المفتاح", ID_EDIT_COPYKEYNAME
        MENUITEM SEPARATOR
        MENUITEM "&بحث...\tCtrl+F", ID_EDIT_FIND
        MENUITEM "البحث التال&ي\tF3", ID_EDIT_FINDNEXT
    END
    POPUP "&عرض"
    BEGIN
        MENUITEM "شريط الح&الة", ID_VIEW_STATUSBAR
        MENUITEM SEPARATOR
        MENUITEM "ت&قسيم", ID_VIEW_SPLIT
        MENUITEM SEPARATOR
        MENUITEM "&تحديث\tF5", ID_VIEW_REFRESH
    END
    POPUP "الم&فضلة"
    BEGIN
        MENUITEM "&إضافة إلى المفضلة", ID_FAVOURITES_ADDTOFAVOURITES, GRAYED
        MENUITEM "&إزالة من المفضلة", ID_FAVOURITES_REMOVEFAVOURITE, GRAYED
    END
    POPUP "&مساعدة"
    BEGIN
        MENUITEM "&مواضيع المساعدة\tF1", ID_HELP_HELPTOPICS
        MENUITEM SEPARATOR
        MENUITEM "&حول محرر التسجيل", ID_HELP_ABOUT
    END
END

