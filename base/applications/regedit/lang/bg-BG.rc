        MENUITEM SEPARATOR
        MENUITEM "&Права...", ID_EDIT_PERMISSIONS
        MENUITEM SEPARATOR
        MENUITEM "Из&триване\tDel", ID_EDIT_DELETE
        MENUITEM "Пре&именуване", ID_EDIT_RENAME
