        MENUITEM "От&печатване...\tCtrl+P", ID_REGISTRY_PRINT, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "Из&ход", ID_REGISTRY_EXIT
    END
    POPUP "&Обработка"
    BEGIN
        MENUITEM "&Промяна", ID_EDIT_MODIFY
        MENUITEM SEPARATOR
        POPUP "&Нов"
        BEGIN
            MENUITEM "&Ключ", ID_EDIT_NEW_KEY
            MENUITEM SEPARATOR
