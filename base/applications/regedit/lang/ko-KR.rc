    BEGIN
        MENUITEM "도움말 목차(&H)\tF1", ID_HELP_HELPTOPICS
        MENUITEM SEPARATOR
        MENUITEM "레지스트리 에디터 정보(&A)", ID_HELP_ABOUT
    END
END

