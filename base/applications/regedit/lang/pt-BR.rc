    POPUP "&Exibir"//FIXME: accelerator collision &E
    BEGIN
        MENUITEM "Barra de &status", ID_VIEW_STATUSBAR
        MENUITEM SEPARATOR
        MENUITEM "&Dividir", ID_VIEW_SPLIT
        MENUITEM SEPARATOR
        MENUITEM "Atuali&zar\tF5", ID_VIEW_REFRESH
    END
    POPUP "&Favoritos"
