        MENUITEM "&Właściwości", IDM_PROPERTIES
        MENUITEM "&Sterowniki zaawansowane", IDM_ADVANCED_CONTROLS
        MENUITEM SEPARATOR
        MENUITEM "Zakoń&cz", IDM_EXIT
    END
