LANGUAGE LANG_LITHUANIAN, SUBLANG_DEFAULT

IDM_MAINMENU MENU
BEGIN
    POPUP "&Parametrai"
    BEGIN
        MENUITEM "&Nustatymai", IDM_PROPERTIES
        MENUITEM "Papildomi &valdymo elementai", IDM_ADVANCED_CONTROLS
        MENUITEM SEPARATOR
        MENUITEM "&Baigti", IDM_EXIT
    END
    POPUP "Pa&galba"
    BEGIN
        MENUITEM "&Pagalba", IDM_HELP_TOPICS
        MENUITEM SEPARATOR
        MENUITEM "&Apie garso valdiklį", IDM_ABOUT
    END
END

STRINGTABLE
BEGIN
    IDS_SNDVOL32 "Garso valdiklis"
    IDS_NOMIXERDEVICES "Šiuo metu nėra aktyvių garso maišymo įrenginių! Programa darbą baigė."
END

