LANGUAGE LANG_CHINESE, SUBLANG_CHINESE_SIMPLIFIED

IDM_MAINMENU MENU
BEGIN
    POPUP "选项(&P)"
    BEGIN
        MENUITEM "属性(&R)", IDM_PROPERTIES
        MENUITEM "高级控制(&A)", IDM_ADVANCED_CONTROLS
        MENUITEM SEPARATOR
        MENUITEM "退出(&X)", IDM_EXIT
    END
    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "帮助主题(&T)", IDM_HELP_TOPICS
        MENUITEM SEPARATOR
        MENUITEM "关于音量控制(&A)", IDM_ABOUT
    END
END

STRINGTABLE
BEGIN
    IDS_SNDVOL32 "音量控制"
    IDS_NOMIXERDEVICES "没有可用的混音器设备！程序即将退出。"
    IDS_SELECT "选择(&S)"
    IDS_ADVANCED_CONTROLS "%s 的高级控制选项"
    IDS_OTHER_CONTROLS1 "&1  %s"
    IDS_OTHER_CONTROLS2 "&2  %s"
END

IDD_PREFERENCES DIALOGEX 0, 0, 224, 250
STYLE DS_SHELLFONT | DS_MODALFRAME | DS_CONTEXTHELP | WS_POPUP | WS_VISIBLE | WS_CAPTION | WS_SYSMENU
CAPTION "属性"
FONT 9, "宋体"
BEGIN
    LTEXT "混音设备(&M)：", -1, 7, 8, 48, 9
