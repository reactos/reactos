LANGUAGE LANG_ITALIAN, SUBLANG_NEUTRAL

IDM_MAINMENU MENU
BEGIN
    POPUP "O&pzioni"
    BEGIN
        MENUITEM "P&roprietà", IDM_PROPERTIES
        MENUITEM "&Controlli avanzati", IDM_ADVANCED_CONTROLS
        MENUITEM SEPARATOR
        MENUITEM "E&sci", IDM_EXIT
    END
    POPUP "&Aiuto"
    BEGIN
        MENUITEM "&Guida in linea", IDM_HELP_TOPICS
        MENUITEM SEPARATOR
        MENUITEM "&Informazioni su Controllo Volume", IDM_ABOUT
    END
END

STRINGTABLE
BEGIN
    IDS_SNDVOL32       "Controllo Volume"
