LANGUAGE LANG_HEBREW, SUBLANG_DEFAULT

IDM_MAINMENU MENU
BEGIN
    POPUP "אפשרויות"
    BEGIN
        MENUITEM "מאפיינים", IDM_PROPERTIES
        MENUITEM "בקרים מתקדמים", IDM_ADVANCED_CONTROLS
        MENUITEM SEPARATOR
        MENUITEM "יציאה", IDM_EXIT
    END
    POPUP "עזרה"
    BEGIN
        MENUITEM "נושאי עזרה", IDM_HELP_TOPICS
        MENUITEM SEPARATOR
        MENUITEM "אודות בקרת עוצמה", IDM_ABOUT
    END
END

STRINGTABLE
BEGIN
    IDS_SNDVOL32 "בקרת עוצמה"
    IDS_NOMIXERDEVICES "אין התקני ערבול פעילים זמינים! היישום יצא עכשיו"
END

