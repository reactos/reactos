LANGUAGE LANG_GERMAN, SUBLANG_NEUTRAL

IDM_MAINMENU MENU
BEGIN
    POPUP "&Optionen"
    BEGIN
        MENUITEM "&Eigenschaften", IDM_PROPERTIES
        MENUITEM "Erweiterte Ein&stellungen", IDM_ADVANCED_CONTROLS
        MENUITEM SEPARATOR
        MENUITEM "&Beenden", IDM_EXIT
    END
    POPUP "&?"
    BEGIN
        MENUITEM "&Hilfethemen", IDM_HELP_TOPICS
        MENUITEM SEPARATOR
        MENUITEM "&Info", IDM_ABOUT
    END
END

