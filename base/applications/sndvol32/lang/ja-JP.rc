LANGUAGE LANG_JAPANESE, SUBLANG_DEFAULT

IDM_MAINMENU MENU
BEGIN
    POPUP "オプション(&P)"
    BEGIN
        MENUITEM "プロパティ(&R)", IDM_PROPERTIES
        MENUITEM "トーン調整(&A)", IDM_ADVANCED_CONTROLS
        MENUITEM SEPARATOR
        MENUITEM "音量調整の終了(&X)", IDM_EXIT
    END
    POPUP "ヘルプ(&H)"
    BEGIN
        MENUITEM "トピックの検索(&H)", IDM_HELP_TOPICS
        MENUITEM SEPARATOR
        MENUITEM "バージョン情報(&A)...", IDM_ABOUT
    END
END

STRINGTABLE
BEGIN
    IDS_SNDVOL32 "音量調整"
    IDS_NOMIXERDEVICES "アクティブなミキサーが利用できません ! アプリケーションを終了します。"
    IDS_SELECT "&Select"
    IDS_ADVANCED_CONTROLS "%s の高度な調整"
    IDS_OTHER_CONTROLS1 "&1  %s"
    IDS_OTHER_CONTROLS2 "&2  %s"
END

