    IDS_STRDUR "Lungime: %.2f s"
    IDS_STRBUF "Tampon: %.2f ko"
    IDS_STRFMT "%.1f kHz %u biți"
    IDS_STRMONO "mono"
    IDS_STRSTEREO "stereo"
    IDS_STRCHAN "%s"
END
