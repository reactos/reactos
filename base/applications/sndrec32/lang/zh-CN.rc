    BEGIN
        MENUITEM "新建",                       ID_NEW
        MENUITEM "打开...",                    ID_FILE_OPEN
        MENUITEM "保存",                       ID_FILE_SAVE, GRAYED
        MENUITEM "另存为...",                  ID_FILE_SAVEAS, GRAYED
