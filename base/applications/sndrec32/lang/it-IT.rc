    IDS_STRPOS "Posizione: %.2f s"
    IDS_STRDUR "Durata: %.2f s"
    IDS_STRBUF "Buffer: %.2f kb"
    IDS_STRFMT "%.1f kHz %u bits"
    IDS_STRMONO "mono"
    IDS_STRSTEREO "stereo"
    IDS_STRCHAN "%s"
END
