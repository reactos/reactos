    IDS_STRDUR "長さ: %.2f 秒"
    IDS_STRBUF "バッファ: %.2f kb"
    IDS_STRFMT "%.1f kHz %u ビット"
    IDS_STRMONO "モノラル"
    IDS_STRSTEREO "ステレオ"
    IDS_STRCHAN "%s"
END
