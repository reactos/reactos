LANGUAGE LANG_ARABIC, SUBLANG_ARABIC_SAUDI_ARABIA

MAIN_MENU MENU
{
    POPUP "&ملف" {
        MENUITEM "&فتح...", MNID_FILE_OPEN
        MENUITEM SEPARATOR
        MENUITEM "&طباعة...", MNID_FILE_PRINT
        MENUITEM "إعدادات الطاب&عة...", MNID_FILE_SETUP
        MENUITEM SEPARATOR
        MENUITEM "خ&روج", MNID_FILE_EXIT
    }
    POPUP "&تحرير" {
        MENUITEM "&نسخ", MNID_EDIT_COPYDLG
        MENUITEM SEPARATOR
        MENUITEM "&تعليق...", MNID_EDIT_ANNOTATE
    }
    POPUP "&إشارة مرجعية" {
        MENUITEM "&تعريف...", MNID_BKMK_DEFINE
    }
    POPUP "&خيارات" {
        MENUITEM "دائماً في الأ&على", MNID_HELP_HELPTOP
        MENUITEM "السجل",           MNID_OPTS_HISTORY
        POPUP    "الخطوط"
        BEGIN
            MENUITEM "صغير",           MNID_OPTS_FONTS_SMALL
            MENUITEM "عادي",           MNID_OPTS_FONTS_NORMAL
            MENUITEM "كبير",           MNID_OPTS_FONTS_LARGE
        END
    }
    POPUP "&مساعدة" {
        MENUITEM "&مساعدة حول المساعدة\tF1", MNID_HELP_HELPON
        MENUITEM "&حول عارض المساعدة", MNID_HELP_ABOUT
    }
}

CONTEXT_MENU MENU
BEGIN
    POPUP ""
    BEGIN
        MENUITEM "تعليق...",           MNID_CTXT_ANNOTATE
        MENUITEM "نسخ",                MNID_CTXT_COPY
        MENUITEM "طباعة...",           MNID_CTXT_PRINT
        POPUP    "الخطوط"
        BEGIN
            MENUITEM "صغير",           MNID_CTXT_FONTS_SMALL
            MENUITEM "عادي",           MNID_CTXT_FONTS_NORMAL
            MENUITEM "كبير",           MNID_CTXT_FONTS_LARGE
        END
    END
END

