	  MENUITEM "Biner\tF8",       ID_CALC_NS_BIN
      MENUITEM SEPARATOR
	  MENUITEM "Qword\tF12",      ID_CALC_WS_QWORD
	  MENUITEM "Dword\tF2",       ID_CALC_WS_DWORD
	  MENUITEM "Word\tF3",        ID_CALC_WS_WORD
	  MENUITEM "Byte\tF4",        ID_CALC_WS_BYTE
