/* Menus */

IDR_MENU_SCIENTIFIC_1 MENU
BEGIN
    POPUP "ت&حرير"
    BEGIN
        MENUITEM "&نسخ\tCtrl+C", IDM_EDIT_COPY
        MENUITEM "ل&صق\tCtrl+V", IDM_EDIT_PASTE
    END
    POPUP "عر&ض"
    BEGIN
        MENUITEM "&قياسي", IDM_VIEW_STANDARD
        MENUITEM "&علمي", IDM_VIEW_SCIENTIFIC
        MENUITEM "ت&حويل", IDM_VIEW_CONVERSION
        MENUITEM SEPARATOR
        MENUITEM "ست &عشري\tF5", IDM_VIEW_HEX, CHECKED
        MENUITEM "&عشري\tF6", IDM_VIEW_DEC, CHECKED
        MENUITEM "&ثماني\tF7", IDM_VIEW_OCT, CHECKED
        MENUITEM "&ثنائي\tF8", IDM_VIEW_BIN, CHECKED
        MENUITEM SEPARATOR
        MENUITEM "&درجات\tF2", IDM_VIEW_DEG, CHECKED
        MENUITEM "&راديان\tF3", IDM_VIEW_RAD, CHECKED
        MENUITEM "&تدرج\tF4", IDM_VIEW_GRAD, CHECKED
        MENUITEM SEPARATOR
        MENUITEM "تجميع الأر&قام", IDM_VIEW_GROUP, CHECKED
    END
    POPUP "م&ساعدة"
    BEGIN
        MENUITEM "مواضيع الم&ساعدة", IDM_HELP_HELP
        MENUITEM SEPARATOR
        MENUITEM "&حول حاسبة ريأكت أو إس", IDM_HELP_ABOUT
    END
END

