/* Menus */

IDR_MENU_SCIENTIFIC_1 MENU
BEGIN
    POPUP "&Szerkesztés"
    BEGIN
        MENUITEM "&Másolás\tCtrl+C", IDM_EDIT_COPY
        MENUITEM "&Beillesztés\tCtrl+V", IDM_EDIT_PASTE
    END
