        MENUITEM "Copier\tCtrl-C", IDM_EDIT_COPY
        MENUITEM "Coller\tCtrl-V", IDM_EDIT_PASTE
    END
    POPUP "Affichage"
    BEGIN
        MENUITEM "Standard", IDM_VIEW_STANDARD
        MENUITEM "Scientifique", IDM_VIEW_SCIENTIFIC
        MENUITEM "Conversion", IDM_VIEW_CONVERSION
