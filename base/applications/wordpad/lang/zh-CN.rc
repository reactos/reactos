    DEFPUSHBUTTON "添加(&A)", ID_TAB_ADD, 20, 80, 45, 15
    PUSHBUTTON "删除(&R)", ID_TAB_DEL, 72, 80, 45, 15
    PUSHBUTTON "确定(&O)", IDOK, 135, 15, 58, 15
    PUSHBUTTON "取消(&C)", IDCANCEL, 135, 33, 58, 15
    PUSHBUTTON "全部删除(&R)", ID_TAB_EMPTY, 135, 51, 58, 15
END

