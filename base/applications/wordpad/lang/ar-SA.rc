        MENUITEM SEPARATOR
        MENUITEM "&خروج",                 ID_FILE_EXIT
    END
    POPUP "&تحرير"
    BEGIN
        MENUITEM "&تراجع\tCtrl+Z",               ID_EDIT_UNDO
        MENUITEM "إعا&دة\tCtrl+Y",               ID_EDIT_REDO
        MENUITEM SEPARATOR
        MENUITEM "&قص\tCtrl+X",                ID_EDIT_CUT
        MENUITEM "&نسخ\tCtrl+C",               ID_EDIT_COPY
        MENUITEM "&لصق\tCtrl+V",              ID_EDIT_PASTE
        MENUITEM "&مسح\tDel",                 ID_EDIT_CLEAR
