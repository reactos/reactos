IDM_MAINMENU MENU
BEGIN
    POPUP "&ملف"
    BEGIN
        MENUITEM "&جديد...\tCtrl+N",       ID_FILE_NEW
        MENUITEM "&فتح...\tCtrl+O",      ID_FILE_OPEN
        MENUITEM "&حفظ\tCtrl+S",         ID_FILE_SAVE
