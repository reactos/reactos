        MENUITEM "البحث عن التا&لي\tF3",              ID_FIND_NEXT
        MENUITEM "&استبدال...\tCtrl+H",         ID_REPLACE
        MENUITEM SEPARATOR
        MENUITEM "للقراءة &فقط",                  ID_EDIT_READONLY
        MENUITEM "&معدل",                   ID_EDIT_MODIFIED
        MENUITEM SEPARATOR
        POPUP "إ&ضافات"
        BEGIN
            MENUITEM "معلومات التحدي&د",             ID_EDIT_SELECTIONINFO
            MENUITEM "تنسيق الح&رف",           ID_EDIT_CHARFORMAT
            MENUITEM "تنسيق الحرف ال&افتراضي",           ID_EDIT_DEFCHARFORMAT
            MENUITEM "تنسيق الفق&رة",           ID_EDIT_PARAFORMAT
            MENUITEM "&الحصول على النص",                   ID_EDIT_GETTEXT
        END
    END
    POPUP "&عرض"
    BEGIN
        MENUITEM "&شريط الأدوات",        ID_TOGGLE_TOOLBAR
        MENUITEM "شريط الت&نسيق",     ID_TOGGLE_FORMATBAR
