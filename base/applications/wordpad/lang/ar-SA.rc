        MENUITEM "شريط الح&الة",     ID_TOGGLE_STATUSBAR
        MENUITEM SEPARATOR
        MENUITEM "خ&يارات...",     ID_VIEWPROPERTIES
    END
    POPUP "&إدراج"
    BEGIN
        MENUITEM "&التاريخ والوقت...",  ID_DATETIME
    END
    POPUP "ت&نسيق"
    BEGIN
        MENUITEM "&خط...",                     ID_FONTSETTINGS
        MENUITEM "&نقاط تعداد",               ID_BULLET
        MENUITEM "&فقرة...",                ID_PARAFORMAT
        MENUITEM "&علامات الجدولة...",                     ID_TABSTOPS
        POPUP "خلفي&ة"
        BEGIN
            MENUITEM "الن&ظام\tCtrl+1",         ID_BACK_1
            MENUITEM "أصفر &فاتح\tCtrl+2",    ID_BACK_2
        END
    END
    POPUP "&مساعدة"
    BEGIN
        MENUITEM "&حول Wine Wordpad",      ID_ABOUT
    END
END

