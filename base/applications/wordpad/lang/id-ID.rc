CAPTION "Tanggal dan waktu"
FONT 8, "MS Shell Dlg"
BEGIN
    LTEXT        "Format tersedia",-1,3,2,100,15
    LISTBOX      IDC_DATETIME,3,12,80,65,LBS_NOINTEGRALHEIGHT | LBS_NOTIFY
    DEFPUSHBUTTON "OK",IDOK,87,12,40,12
    PUSHBUTTON   "Batal",IDCANCEL,87,26,40,12
END

