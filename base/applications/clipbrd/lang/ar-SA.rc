LANGUAGE LANG_ARABIC, SUBLANG_ARABIC_SAUDI_ARABIA

ID_ACCEL ACCELERATORS
BEGIN
    VK_DELETE, CMD_DELETE, VIRTKEY
END

MAIN_MENU MENU
BEGIN
    POPUP "&ملف"
    BEGIN
        MENUITEM "&فتح...", CMD_OPEN
        MENUITEM "حفظ &باسم...", CMD_SAVE_AS
        MENUITEM SEPARATOR
        MENUITEM "خ&روج", CMD_EXIT
    END
    POPUP "ت&حرير"
    BEGIN
        MENUITEM "&حذف\tDel", CMD_DELETE
    END
    POPUP "&عرض"
    BEGIN
        MENUITEM "&تلقائي", CMD_AUTOMATIC
    END
    POPUP "م&ساعدة"
    BEGIN
        MENUITEM "مواضيع الم&ساعدة", CMD_HELP
        MENUITEM SEPARATOR
        MENUITEM "&حول", CMD_ABOUT
    END
END

