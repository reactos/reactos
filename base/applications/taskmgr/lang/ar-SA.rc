IDR_TASKMANAGER MENU
BEGIN
    POPUP "&ملف"
    BEGIN
        MENUITEM "&مهمة جديدة (تشغيل...)", ID_FILE_NEW
        MENUITEM SEPARATOR
        MENUITEM "إ&نهاء مدير المهام", ID_FILE_EXIT
    END
    POPUP "&خيارات"
    BEGIN
        MENUITEM "&دائماً في المقدمة", ID_OPTIONS_ALWAYSONTOP
        , CHECKED
        MENUITEM "&تصغير عند الاستخدام", ID_OPTIONS_MINIMIZEONUSE
        , CHECKED
        MENUITEM "إ&خفاء عند التصغير", ID_OPTIONS_HIDEWHENMINIMIZED
        , CHECKED
        MENUITEM "إظهار مهام &16 بت", ID_OPTIONS_SHOW16BITTASKS
        , CHECKED
    END
    POPUP "&عرض"
    BEGIN
        MENUITEM "ت&حديث الآن", ID_VIEW_REFRESH
        POPUP "&سرعة التحديث"
        BEGIN
            MENUITEM "&عالية", ID_VIEW_UPDATESPEED_HIGH
            MENUITEM "عا&دية", ID_VIEW_UPDATESPEED_NORMAL
            , CHECKED
            MENUITEM "&منخفضة", ID_VIEW_UPDATESPEED_LOW
            MENUITEM "&متوقف", ID_VIEW_UPDATESPEED_PAUSED
        END
        MENUITEM SEPARATOR
        MENUITEM "رموز &كبيرة", ID_VIEW_LARGE
        MENUITEM "رموز &صغيرة", ID_VIEW_SMALL
        MENUITEM "&تفاصيل", ID_VIEW_DETAILS, CHECKED
        MENUITEM "&تحديد الأعمدة...", ID_VIEW_SELECTCOLUMNS
        POPUP "&سجل وحدة المعالجة"
        BEGIN
            MENUITEM "&معالج واحد", ID_VIEW_CPUHISTORY_ONEGRAPHALL
            MENUITEM "رسم بياني &لكل معالج", ID_VIEW_CPUHISTORY_ONEGRAPHPERCPU
            , CHECKED
        END
        MENUITEM "إظهار أوقات ال&نواة", ID_VIEW_SHOWKERNELTIMES
    END
    POPUP "نوا&فذ"
    BEGIN
        MENUITEM "ترتيب أ&فقي", ID_WINDOWS_TILEHORIZONTALLY
        MENUITEM "ترتيب &عمودي", ID_WINDOWS_TILEVERTICALLY
        MENUITEM "ت&صغير", ID_WINDOWS_MINIMIZE
        MENUITEM "ت&كبير", ID_WINDOWS_MAXIMIZE
        MENUITEM "ت&داخل", ID_WINDOWS_CASCADE
        MENUITEM "جلب للأ&مام", ID_WINDOWS_BRINGTOFRONT
    END
    POPUP "إ&غلاق الكمبيوتر"
    BEGIN
        MENUITEM "وضع الاست&عداد",                  ID_SHUTDOWN_STANDBY
        MENUITEM "إس&بات",                   ID_SHUTDOWN_HIBERNATE
        MENUITEM "إي&قاف التشغيل",                  ID_SHUTDOWN_POWEROFF
        MENUITEM "إعادة الت&شغيل",                   ID_SHUTDOWN_REBOOT
        MENUITEM "تس&جيل الخروج من %s",                ID_SHUTDOWN_LOGOFF
        MENUITEM "ت&بديل المستخدم\tWinKey+L",     ID_SHUTDOWN_SWITCHUSER, GRAYED
        MENUITEM "قفل ال&كمبيوتر\tWinKey+L",      ID_SHUTDOWN_LOCKCOMPUTER
        MENUITEM "قطع الات&صال",                ID_SHUTDOWN_DISCONNECT, GRAYED
        MENUITEM "إ&خراج الكمبيوتر",            ID_SHUTDOWN_EJECT_COMPUTER, GRAYED
    END
    POPUP "م&ساعدة"
    BEGIN
        MENUITEM "مواضيع تعليمات مدير ال&مهام", ID_HELP_TOPICS
        MENUITEM SEPARATOR
        MENUITEM "&حول مدير المهام", ID_HELP_ABOUT
    END
END

