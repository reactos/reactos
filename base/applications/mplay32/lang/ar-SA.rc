LANGUAGE LANG_ARABIC, SUBLANG_ARABIC_SAUDI_ARABIA

IDR_MAINMENU MENU
BEGIN
    POPUP "&ملف"
    BEGIN
        MENUITEM "&فتح...\tCtrl+O", IDM_OPEN_FILE
        MENUITEM "&إغلاق\tCtrl+W", IDM_CLOSE_FILE
        MENUITEM SEPARATOR
        MENUITEM "خ&روج", IDM_EXIT
    END
    POPUP "&تشغيل"
    BEGIN
        MENUITEM "تشغيل/إ&يقاف مؤقت\tCtrl+P", IDC_PLAY
        MENUITEM "إيقا&ف\tCtrl+S", IDC_STOP
        MENUITEM SEPARATOR
        MENUITEM "تكرار\tCtrl+T", IDM_REPEAT
    END
    POPUP "&عرض"
    BEGIN
        MENUITEM "وضع النافذة &الواحدة", IDM_SWITCHVIEW
    END
    POPUP "&جهاز"
    BEGIN
        MENUITEM SEPARATOR
        MENUITEM "&خصائص", IDM_DEVPROPS
        MENUITEM SEPARATOR
        MENUITEM "&التحكم في مستوى الصوت", IDM_VOLUMECTL
    END
    POPUP "&مساعدة"
    BEGIN
        MENUITEM "&حول", IDM_ABOUT
    END
END

ID_ACCELERATORS ACCELERATORS
BEGIN
    "^O", IDM_OPEN_FILE
    "^W", IDM_CLOSE_FILE
    "^P", IDC_PLAY
    "^S", IDC_STOP
    "^T", IDM_REPEAT
END

