LANGUAGE LANG_JAPANESE, SUBLANG_DEFAULT

IDR_MAINMENU MENU
BEGIN
    POPUP "ファイル(&F)"
    BEGIN
        MENUITEM "開く(&O)...", IDM_OPEN_FILE
        MENUITEM "閉じる(&C)", IDM_CLOSE_FILE
        MENUITEM SEPARATOR
        MENUITEM "終了(&E)", IDM_EXIT
    END
    POPUP "ヘルプ(&H)"
    BEGIN
        MENUITEM "バージョン情報(&A)...", IDM_ABOUT
    END
END

STRINGTABLE
