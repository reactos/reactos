    IDS_TOOLTIP_STOP "停止"
    IDS_TOOLTIP_EJECT "取り出し"
    IDS_TOOLTIP_BACKWARD "後ろへスキップ"
    IDS_TOOLTIP_SEEKBACK "後ろへシーク"
    IDS_TOOLTIP_SEEKFORW "前へシーク"
    IDS_TOOLTIP_FORWARD "前へスキップ"
    IDS_APPTITLE "ReactOS マルチメディア プレーヤー"
