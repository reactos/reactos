/* Menus */

IDR_MENU1 MENU
BEGIN
    POPUP "&Гра"
    BEGIN
        MENUITEM "&Роздати карти\tF2", IDM_GAME_NEW
        MENUITEM SEPARATOR
        MENUITEM "&Undo", IDM_GAME_UNDO, GRAYED
        MENUITEM "&Колода...", IDM_GAME_DECK
        MENUITEM "Па&раметри...", IDM_GAME_OPTIONS
        MENUITEM SEPARATOR
        MENUITEM "В&ихід", IDM_GAME_EXIT
    END
    POPUP "&Довідка"
    BEGIN
        MENUITEM "&Зміст\tF1", IDM_HELP_CONTENTS
        MENUITEM "&Про програму...", IDM_HELP_ABOUT
    END
END

/* Accelerators */

IDR_ACCELERATOR1 ACCELERATORS
BEGIN
    VK_F1, IDM_HELP_CONTENTS, VIRTKEY, NOINVERT
    VK_F2, IDM_GAME_NEW, VIRTKEY, NOINVERT
END
