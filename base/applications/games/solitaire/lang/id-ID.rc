/* Strings */

STRINGTABLE
BEGIN
    IDS_SOL_NAME "Solitaire"
    IDS_SOL_ABOUT "Solitaire oleh J Brown\nCardLib version 1.0"
    IDS_SOL_QUIT "Keluar dari permainan saat ini?"
    IDS_SOL_WIN "Selamat, anda menang!"
    IDS_SOL_DEAL "Main lagi?"
    IDS_SOL_SCORE "Nilai: %d"
    IDS_SOL_TIME "Waktu: %d"
END

/* Menus */

IDR_MENU1 MENU
BEGIN
    POPUP "&Permainan"
    BEGIN
        MENUITEM "&Main Baru\tF2", IDM_GAME_NEW
        MENUITEM SEPARATOR
        MENUITEM "&Undo", IDM_GAME_UNDO, GRAYED
        MENUITEM "&Dek...", IDM_GAME_DECK
        MENUITEM "&Pilihan...", IDM_GAME_OPTIONS
        MENUITEM SEPARATOR
        MENUITEM "&Keluar", IDM_GAME_EXIT
    END
    POPUP "&Bantuan"
    BEGIN
        MENUITEM "&Daftar Isi\tF1", IDM_HELP_CONTENTS
        MENUITEM "&Tentang", IDM_HELP_ABOUT
    END
END

/* Accelerators */

IDR_ACCELERATOR1 ACCELERATORS
BEGIN
    VK_F1, IDM_HELP_CONTENTS, VIRTKEY, NOINVERT
    VK_F2, IDM_GAME_NEW, VIRTKEY, NOINVERT
END
