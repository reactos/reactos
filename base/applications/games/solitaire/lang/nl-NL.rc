/* Menus */

IDR_MENU1 MENU
BEGIN
    POPUP "&Spel"
    BEGIN
        MENUITEM "&Delen\tF2", IDM_GAME_NEW
        MENUITEM SEPARATOR
        MENUITEM "&Undo", IDM_GAME_UNDO, GRAYED
        MENUITEM "&Kaarten...", IDM_GAME_DECK
        MENUITEM "&Opties...", IDM_GAME_OPTIONS
        MENUITEM SEPARATOR
        MENUITEM "&Afsluiten", IDM_GAME_EXIT
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "Help-onder&werpen\tF1", IDM_HELP_CONTENTS
        MENUITEM "&Over Solitaire", IDM_HELP_ABOUT
    END
END

/* Accelerators */

IDR_ACCELERATOR1 ACCELERATORS
BEGIN
    VK_F1, IDM_HELP_CONTENTS, VIRTKEY, NOINVERT
    VK_F2, IDM_GAME_NEW, VIRTKEY, NOINVERT
END
