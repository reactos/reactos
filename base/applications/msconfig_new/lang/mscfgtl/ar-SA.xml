<_locDefinition>
  <_locDefault _loc="locNone"/>
  <_locTag _locAttrData="NAME,HELP">TOOL</_locTag>
</_locDefinition>

<MSCONFIGTOOLS>
  <TOOL _locID="About ReactOS" NAME="حول ReactOS" PATH="%windir%\system32\winver.exe" DEFAULT_OPT="" ADV_OPT="" HELP="يعرض معلومات إصدار ReactOS."/>
  <TOOL _locID="Action Center" NAME="مركز الإجراءات" PATH="%windir%\System32\wscui.cpl" DEFAULT_OPT="" ADV_OPT="" HELP="يفتح مركز الإجراءات."/>
  <TOOL _locID="Computer Management" NAME="إدارة الكمبيوتر" PATH="%windir%\System32\compmgmt.msc" DEFAULT_OPT="" ADV_OPT="" HELP="يعرض ويهيئ إعدادات ومكونات النظام."/>
  <TOOL _locID="System Information" NAME="معلومات النظام" PATH="%windir%\System32\msinfo32.exe" DEFAULT_OPT="" ADV_OPT="" HELP="يعرض معلومات متقدمة حول إعدادات الأجهزة والبرامج."/>
  <TOOL _locID="Event Viewer" NAME="عارض الأحداث" PATH="%windir%\System32\eventvwr.exe" DEFAULT_OPT="" ADV_OPT="" HELP="يعرض رسائل المراقبة واستكشاف الأخطاء وإصلاحها."/>
  <TOOL _locID="Programs" NAME="البرامج" PATH="%windir%\System32\appwiz.cpl" DEFAULT_OPT="" ADV_OPT="" HELP="يشغل أو يضيف أو يزيل البرامج ومكونات ReactOS."/>
  <TOOL _locID="System Properties" NAME="خصائص النظام" PATH="%windir%\System32\control.exe" DEFAULT_OPT="system" ADV_OPT="" HELP="يعرض معلومات أساسية حول إعدادات نظام الكمبيوتر الخاص بك."/>
  <TOOL _locID="Internet Options" NAME="خيارات الإنترنت" PATH="%windir%\System32\inetcpl.cpl" DEFAULT_OPT="" ADV_OPT="" HELP="يعرض خصائص الإنترنت."/>
  <TOOL _locID="Internet Protocol Configuration" NAME="تكوين بروتوكول الإنترنت" PATH="%windir%\System32\cmd.exe" DEFAULT_OPT="/k %windir%\system32\ipconfig.exe" ADV_OPT="/k %windir%\system32\ipconfig.exe /all" HELP="يعرض ويهيئ إعدادات عنوان الشبكة."/>
  <TOOL _locID="Performance Monitor" NAME="مراقب الأداء" PATH="%windir%\System32\perfmon.exe" DEFAULT_OPT="" ADV_OPT="/sys" HELP="يراقب أداء أجهزة الكمبيوتر المحلية أو البعيدة."/>
  <TOOL _locID="Resource Monitor" NAME="مراقب الموارد" PATH="%windir%\System32\resmon.exe" DEFAULT_OPT="" ADV_OPT="" HELP="يراقب أداء واستخدام موارد الكمبيوتر المحلي."/>
  <TOOL _locID="Task Manager" NAME="مدير المهام" PATH="%windir%\System32\taskmgr.exe" DEFAULT_OPT="" ADV_OPT="" HELP="يعرض تفاصيل حول البرامج والعمليات التي تعمل على جهاز الكمبيوتر الخاص بك."/>
  <TOOL _locID="Command Prompt" NAME="موجه الأوامر" PATH="%windir%\System32\cmd.exe" DEFAULT_OPT="" ADV_OPT="" HELP="يفتح نافذة موجه الأوامر."/>
  <TOOL _locID="Registry Editor" NAME="محرر التسجيل" PATH="%windir%\System32\regedt32.exe" DEFAULT_OPT="" ADV_OPT="" HELP="يجري تغييرات على سجل ReactOS."/>
  <TOOL _locID="Remote Assistance" NAME="المساعدة عن بعد" PATH="%windir%\System32\msra.exe" DEFAULT_OPT="" ADV_OPT="" HELP="يتلقى المساعدة من (أو يقدم المساعدة إلى) صديق عبر الإنترنت."/>
  <TOOL _locID="System Restore" NAME="استعادة النظام" PATH="%windir%\System32\rstrui.exe" DEFAULT_OPT="" ADV_OPT="" HELP="يستعيد نظام الكمبيوتر الخاص بك إلى حالة سابقة."/>
</MSCONFIGTOOLS>
