    DEFPUSHBUTTON "&OK", 1, 158, 6, 47, 14
    PUSHBUTTON "&Cancelar", 2, 158, 23, 47, 14
    PUSHBUTTON "&Ajuda", 254, 158, 43, 47, 14
END

