    CONTROL "sho&w hidden", ID_SHOW_HIDDEN_ICONS, "Button", BS_AUTOCHECKBOX | WS_TABSTOP, 7, 154, 56, 10
    DEFPUSHBUTTON "&OK", IDOK, 91, 153, 50, 14, WS_GROUP
