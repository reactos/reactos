IDM_STARTMENU MENUEX
BEGIN
    POPUP ""
    BEGIN
        MENUITEM "", -1, MFT_SEPARATOR
        POPUP "&Programak", IDM_PROGRAMS
        BEGIN
            MENUITEM "(Vacío)", -1, MFT_STRING, MFS_GRAYED
        END
