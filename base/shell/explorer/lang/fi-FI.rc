IDM_STARTMENU MENUEX
BEGIN
    POPUP ""
    BEGIN
        MENUITEM "", -1, MFT_SEPARATOR
        POPUP "&Ohjelmat", IDM_PROGRAMS
        BEGIN
            MENUITEM "(Tyhjä)", -1, MFT_STRING, MFS_GRAYED
        END
        POPUP "&Suosikit", IDM_FAVORITES
        BEGIN
            MENUITEM "(Tyhjä)", -1, MFT_STRING, MFS_GRAYED
        END
        POPUP "&Tiedostot", IDM_DOCUMENTS
        BEGIN
            MENUITEM "(Tyhjä)", -1, MFT_STRING, MFS_GRAYED
