        BEGIN
            MENUITEM "(Boş)", -1, MFT_STRING, MFS_GRAYED
        END
        POPUP "&Belgeler", IDM_DOCUMENTS
        BEGIN
            MENUITEM "(Boş)", -1, MFT_STRING, MFS_GRAYED
        END
        POPUP "&Ayarlar", IDM_SETTINGS
