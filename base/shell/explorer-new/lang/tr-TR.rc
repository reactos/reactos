IDM_STARTMENU MENUEX
BEGIN
    POPUP ""
    BEGIN
        MENUITEM "", -1, MFT_SEPARATOR
        POPUP "&İzlenceler", IDM_PROGRAMS
        BEGIN
            MENUITEM "(Boş)", -1, MFT_STRING, MFS_GRAYED
        END
