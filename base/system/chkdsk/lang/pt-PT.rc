LANGUAGE LANG_PORTUGUESE, SUBLANG_NEUTRAL

STRINGTABLE
BEGIN
    IDS_USAGE "\n\
Uso: %s [unidade:] [-F] [-V] [-R] [-C]\n\n\
[unidade:]  Especifica a unidade a verificar.\n\
-F          Corrige os erros no disco.\n\
-V          Mostra o caminho completo de cada ficheiro no disco.\n\
-R          Localiza sectores danificados e recupera informações legíveis.\n\
-C          Verifica a unidade sómente se existirem alterações desde a\n\
            ultima verificação.\n\
\n"

    IDS_PERCENT_COMPL "%d percento completo.\r"
    IDS_FILE_SYSTEM "O sistema de ficheiros é %s.\n"
    IDS_ABOUT "\n\
Chkdskx v1.0.1 by Mark Russinovich\n\
Systems Internals - http://www.sysinternals.com\n\
ReactOS adaptation 1999 by Emanuele Aliberti\n\n"
END

