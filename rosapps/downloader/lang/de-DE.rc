  EDITTEXT IDC_UPDATE_SERVER_EDIT, 6, 65, 255, 14, WS_VISIBLE | WS_TABSTOP
  DEFPUSHBUTTON "&OK", IDOK, 147, 90, 54, 15
  PUSHBUTTON "&Abbrechen", IDCANCEL, 207, 90, 54, 15
END

